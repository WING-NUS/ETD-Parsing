<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" page-range-format="minimal" demote-non-dropping-particle="sort-only" default-locale="en-US">
  <info>
    <title>American Journal of Sonography</title>
    <title-short>AJS</title-short>
    <id>http://www.zotero.org/styles/american-journal-of-sonography</id>
    <link href="http://www.zotero.org/styles/american-journal-of-sonography" rel="self"/>
    <link href="http://www.zotero.org/styles/journal-of-pharmacy-and-pharmacology" rel="template"/>
    <link href="https://americanjs.com/instructions-for-authors.php" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
    </author>
    <category citation-format="numeric"/>
    <category field="medicine"/>
    <issn>2572-4711</issn>
    <eissn>2572-472X</eissn>
