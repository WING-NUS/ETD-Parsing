<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" default-locale="en-US" version="1.0" demote-non-dropping-particle="sort-only" page-range-format="expanded">
  <info>
    <title>American Nuclear Society</title>
    <title-short>ANS</title-short>
    <id>http://www.zotero.org/styles/american-nuclear-society</id>
    <link href="http://www.zotero.org/styles/american-nuclear-society" rel="self"/>
    <link href="http://www.zotero.org/styles/spie-journals" rel="template"/>
    <link href="http://cdn.ans.org/pubs/journals/fst/docs/fst-ref-style.pdf" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
    </author>
    <category citation-format="numeric"/>
    <category field="physics"/>
    <category field="engineering"/>
    <updated>2017-09-19T14:22:43+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="presented at">presented at</term>
      <term name="accessed">current as of</term>
    </terms>
  </locale>
  <macro name="type-number">
    <group delimiter=" ">
      <choose>
        <if type="patent" match="none">
          <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
        </if>
      </choose>
      <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group prefix="&lt;edition&gt;, " delimiter=" " suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short"/>
        </group>
      </if>
      <else>
        <text variable="edition" prefix="&lt;edition&gt;, " suffix="&lt;/edition&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name initialize-with=". " delimiter=", " and="text"/>
      <label form="short" prefix=", " text-case="title"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name initialize-with=". " delimiter=", " and="text">
        <name-part name="family" text-case="uppercase"/>
      </name>
      <label form="short" prefix=", " text-case="title"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if type="report">
        <text variable="title" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else-if type="bill book graphic legal_case legislation motion_picture report song" match="any">
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        <text macro="edition"/>
      </else-if>
      <else>
        <text variable="title" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="thesis">
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </if>
      <else>
        <group delimiter=", ">
          <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text variable="DOI" prefix="&lt;DOI&gt;; https://doi.org/" suffix="&lt;/DOI&gt;"/>
      </if>
      <else-if type="webpage">
        <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
      </else-if>
    </choose>
  </macro>
  <macro name="issued">
    <date variable="issued">
      <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
      <date-part name="month" form="long" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
      <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <macro name="issued-year">
    <date variable="issued" prefix=" (" suffix=")">
      <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <macro name="accessed">
    <group delimiter=" " prefix="(" suffix=")">
      <text term="accessed"/>
      <date variable="accessed">
        <date-part name="month" form="short" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
        <date-part name="day" prefix="&lt;day&gt; " suffix="&lt;/day&gt;"/>
        <date-part name="year" prefix="&lt;year&gt;, " suffix="&lt;/year&gt;"/>
      </date>
    </group>
  </macro>
  <macro name="page">
    <choose>
      <if type="article-journal">
        <text variable="page-first" prefix="&lt;page-first&gt;" suffix="&lt;/page-first&gt;"/>
      </if>
      <else>
        <group delimiter=" ">
          <label variable="page" form="short"/>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="container-prefix">
    <choose>
      <if type="webpage article-journal" match="none">
        <text term="in" suffix=" "/>
      </if>
    </choose>
  </macro>
  <macro name="locators">
    <group delimiter=" ">
      <text variable="volume" prefix="&lt;volume&gt; " font-weight="bold" suffix="&lt;/volume&gt;"/>
      <text variable="issue" prefix="&lt;issue&gt;" suffix="&lt;/issue&gt;"/>
    </group>
  </macro>
  <macro name="container">
    <choose>
      <if variable="container-title">
        <text macro="container-prefix"/>
        <choose>
          <if type="article-journal">
            <text variable="container-title" form="short" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </if>
          <else>
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </else>
        </choose>
        <text macro="locators"/>
        <text macro="edition"/>
      </if>
      <else-if type="paper-conference">
        <group delimiter=" ">
          <text term="presented at"/>
          <text variable="event" prefix="&lt;event&gt;" suffix="&lt;/event&gt;"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout vertical-align="sup" delimiter=",">
      <text variable="citation-number" prefix="&lt;citation-number&gt;" suffix="&lt;/citation-number&gt;"/>
    </layout>
  </citation>
  <bibliography et-al-min="4" et-al-use-first="1" entry-spacing="0" second-field-align="flush">
    <layout suffix=".">
      <text variable="citation-number" suffix=".&lt;/citation-number&gt;" prefix="&lt;citation-number&gt;"/>
      <text macro="author" suffix=", "/>
      <choose>
        <if type="webpage">
          <group delimiter="; ">
            <group delimiter=", ">
              <text macro="title"/>
              <text macro="container"/>
            </group>
            <text macro="issued"/>
            <text macro="access"/>
            <text macro="accessed"/>
          </group>
        </if>
        <else-if type="report">
          <group delimiter=", ">
            <text macro="title"/>
            <text macro="type-number"/>
            <text macro="publisher"/>
            <text macro="page"/>
          </group>
          <text macro="issued-year"/>
          <text macro="access"/>
        </else-if>
        <else-if type="paper-conference">
          <choose>
            <if variable="container-title">
              <group delimiter=", ">
                <text macro="title"/>
                <text macro="container"/>
                <text macro="editor"/>
                <text macro="page"/>
                <text macro="publisher"/>
              </group>
              <text macro="issued-year"/>
            </if>
            <else>
              <group delimiter=", ">
                <text macro="title"/>
                <text macro="container"/>
                <text macro="issued"/>
                <text variable="event-place" prefix="&lt;event-place&gt;" suffix="&lt;/event-place&gt;"/>
                <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
                <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
              </group>
            </else>
          </choose>
          <text macro="access"/>
        </else-if>
        <else-if type="interview">
          <group delimiter=", ">
            <text variable="note" prefix="&lt;note&gt;" suffix="&lt;/note&gt;"/>
            <text variable="medium" prefix="&lt;medium&gt;" suffix="&lt;/medium&gt;"/>
          </group>
          <text macro="issued-year"/>
        </else-if>
        <else>
          <group delimiter=", ">
            <text macro="title"/>
            <text macro="type-number"/>
            <text macro="container"/>
            <text macro="editor"/>
            <text macro="page"/>
            <text macro="publisher"/>
          </group>
          <text macro="issued-year"/>
          <text macro="access"/>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>