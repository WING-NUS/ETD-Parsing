  <macro name="edition">
    <number suffix=" &lt;/edition&gt;" variable="edition" form="ordinal" prefix="&lt;edition&gt;"/>
    <label variable="edition" form="short"/>
  </macro>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name name-as-sort-order="all" sort-separator=" " initialize-with="" delimiter=", " delimiter-precedes-last="always"/>
      <label form="short" text-case="lowercase" strip-periods="true" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="text" initialize-with=""/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
        <text variable="title" form="short" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="report" match="none">
        <group delimiter=", " suffix=", ">
          <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          <text variable="publisher-place" strip-periods="true" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="page">
    <choose>
      <if type="article article-journal" match="any">
        <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
      </if>
      <else-if type="bill book report thesis review-book" match="any">
        <choose>
          <if match="any" variable="number-of-pages">
            <group delimiter=" ">
              <text variable="number-of-pages" prefix="&lt;number-of-pages&gt;" suffix="&lt;/number-of-pages&gt;"/>
              <label strip-periods="true" variable="number-of-pages" form="short"/>
            </group>
          </if>
          <else>
            <group delimiter=" ">
              <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
              <label plural="always" strip-periods="true" variable="page" form="short"/>
            </group>
          </else>
        </choose>
      </else-if>
      <else>
        <group>
          <label strip-periods="true" suffix=" " variable="page" form="short"/>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
        </group>
      </else>
    </choose>
  </macro>
  <citation and="text" et-al-min="3" et-al-use-first="1" disambiguate-add-names="true" disambiguate-add-year-suffix="true" collapse="year-suffix" year-suffix-delimiter=", ">
    <sort>
      <key variable="issued"/>
      <key variable="author"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group delimiter=" ">
        <text macro="author-short"/>
        <group>
          <date variable="issued">
            <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
          </date>
          <text variable="locator" prefix="&lt;locator&gt;: " suffix="&lt;/locator&gt;"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="author-short"/>
      <key variable="issued"/>
    </sort>
    <layout>
      <choose>
        <if type="webpage" match="any">
          <group delimiter=" " suffix=". ">
            <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
              <substitute>
                <names variable="interviewer illustrator container-author" prefix="&lt;interviewer illustrator container-author&gt;" suffix="&lt;/interviewer illustrator container-author&gt;"/>
                <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
              </substitute>
            </names>
            <text variable="title" text-case="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
            <date date-parts="year" form="text" variable="issued" prefix=" (" suffix=")">
              <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
            </date>
          </group>
          <group suffix=". ">
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </group>
          <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
          <group delimiter=" " prefix=" (" suffix=")">
            <text term="accessed" text-case="capitalize-first"/>
            <date variable="accessed">
              <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
              <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
              <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
            </date>
          </group>
        </if>
        <else>
          <choose>
            <if match="none" variable="author">
              <choose>
                <if type="book" match="any" variable="editor">
                  <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
                    <name initialize-with="" name-as-sort-order="all" sort-separator=" "/>
                    <label form="short" strip-periods="true" prefix=" (" suffix=")"/>
                  </names>
                </if>
                <else>
                  <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
                </else>
              </choose>
            </if>
            <else>
              <text macro="author"/>
            </else>
          </choose>
          <date variable="issued" prefix=" (" suffix=") ">
            <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
          </date>
          <choose>
            <if type="bill graphic legal_case legislation motion_picture song" match="any">
              <group delimiter=" ">
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <group delimiter=", " suffix=". ">
                  <text macro="edition"/>
                  <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                  <group>
                    <label plural="never" text-case="capitalize-first" suffix=" " variable="volume"/>
                    <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                  </group>
                </group>
              </group>
            </if>
            <else-if type="book" match="any">
              <choose>
                <if match="any" variable="collection-title">
                  <text variable="title" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
                  <text variable="collection-title" suffix=". &lt;/collection-title&gt;" prefix="&lt;collection-title&gt;"/>
                </if>
                <else>
                  <group delimiter=", " suffix=". ">
                    <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                    <text macro="edition"/>
                    <group>
                      <label text-case="capitalize-first" suffix=" " variable="volume"/>
                      <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                    </group>
                  </group>
                </else>
              </choose>
            </else-if>
            <else-if type="thesis">
              <text variable="title" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
              <text variable="genre" strip-periods="true" suffix=" Thesis. &lt;/genre&gt;" prefix="&lt;genre&gt;"/>
            </else-if>
            <else-if type="chapter" match="any">
              <text variable="title" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
              <group suffix=", ">
                <text term="in" text-case="capitalize-first" suffix=": "/>
                <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
                  <name initialize-with="" name-as-sort-order="all" sort-separator=" "/>
                  <label form="short" strip-periods="true" prefix=" (" suffix=")"/>
                </names>
              </group>
              <text variable="container-title" text-case="title" suffix=". &lt;/container-title&gt;" prefix="&lt;container-title&gt;"/>
              <group delimiter=", " suffix=". ">
                <group delimiter=" ">
                  <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                  <text variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
                </group>
                <text macro="edition"/>
              </group>
            </else-if>
            <else-if type="paper-conference" match="any">
              <text variable="title" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
              <group delimiter=", " prefix=" " suffix=". ">
                <text variable="container-title" text-case="title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
              </group>
              <text variable="event" suffix=", &lt;/event&gt;" prefix="&lt;event&gt;"/>
              <text variable="event-place" suffix=", &lt;/event-place&gt;" prefix="&lt;event-place&gt;"/>
            </else-if>
            <else-if type="report" match="any">
              <text variable="title" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
              <text variable="collection-title" suffix=" &lt;/collection-title&gt;" prefix="&lt;collection-title&gt;"/>
              <group delimiter=", " suffix=", ">
                <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
                <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
                <date form="text" date-parts="year-month-day" variable="issued" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
              </group>
            </else-if>
            <else>
              <text variable="title" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
              <text variable="container-title" strip-periods="true" font-style="italic" suffix=" &lt;/container-title&gt;" prefix="&lt;container-title&gt;"/>
              <group>
                <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                <text variable="issue" form="short" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
              </group>
            </else>
          </choose>
          <text macro="publisher"/>
          <text macro="page"/>
          <choose>
            <if match="any" variable="DOI">
              <text variable="DOI" prefix="&lt;DOI&gt;, doi: " suffix="&lt;/DOI&gt;"/>
            </if>
          </choose>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>