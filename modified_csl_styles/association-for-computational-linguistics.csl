<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="never" default-locale="en-US">
  <info>
    <title>Association for Computational Linguistics - Conference Proceedings</title>
    <title-short>ACL</title-short>
    <id>http://www.zotero.org/styles/association-for-computational-linguistics</id>
    <link href="http://www.zotero.org/styles/association-for-computational-linguistics" rel="self"/>
    <link href="http://www.aclweb.org/" rel="documentation"/>
    
    
    <author>
      <name>Hajime Senuma</name>
      <email>hajime.senuma@gmail.com</email>
      <uri>https://github.com/hajimes</uri>
    </author>
    <category citation-format="author-date"/>
    <category field="engineering"/>
    <category field="linguistics"/>
    <summary>CSL for papers in ACL-related conference proceedings, e.g., ACL-HLT, EACL, NAACL-HTL, and EMNLP. (But NOT for the journal "Computational Linguistics")</summary>
    <published>2013-11-20T21:00:00Z</published>
    <updated>2013-11-20T21:00:00Z</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="long" and="text" delimiter=", "/>
      <substitute>
        <text value=""/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="text" delimiter=", "/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
          <name form="short" and="text" delimiter=", "/>
        </names>
        <text value="mis"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name form="long" and="text" delimiter=", "/>
      <label prefix=", " form="long"/>
    </names>
  </macro>
  <macro name="author-or-editor">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="long" and="text" delimiter=", "/>
      <substitute>
        <text macro="editor"/>
        <text value=""/>
      </substitute>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text value=" "/>
      </else>
    </choose>
  </macro>
  <macro name="month">
    <date variable="issued">
      <date-part name="month" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
    </date>
  </macro>
  <macro name="edition">
    <number variable="edition" form="ordinal" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
    <text term="edition" prefix=" "/>
  </macro>
  <macro name="volume-or-number">
    <choose>
      <if variable="volume">
        <group delimiter=" " prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;">
          <label variable="volume"/>
          <number variable="volume"/>
        </group>
      </if>
      <else-if variable="number">
        <group delimiter=" " prefix="&lt;number&gt;" suffix="&lt;/number&gt;">
          <text value="number"/>
          <number variable="number"/>
        </group>
      </else-if>
      <else-if variable="issue">
        <group delimiter=" " prefix="&lt;issue&gt;" suffix="&lt;/issue&gt;">
          <text value="number"/>
          <number variable="issue"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="event-or-publisher-place">
    <choose>
      <if variable="event-place">
        <text variable="event-place" prefix="&lt;event-place&gt;" suffix="&lt;/event-place&gt;"/>
      </if>
      <else>
        <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="thesis-type">
    <choose>
      <if variable="genre">
        <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
      </if>
      <else>
        <text value="Ph.D. thesis"/>
      </else>
    </choose>
  </macro>
  <macro name="volume-and-collection-title">
    <choose>
      <if variable="volume collection-title" match="all">
        <group delimiter=" " prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;">
          <text term="volume"/>
          <number variable="volume"/>
          <text value="of"/>
          <text variable="collection-title" font-style="italic" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
        </group>
      </if>
      <else-if variable="volume">
        <group delimiter=" " prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;">
          <text term="volume"/>
          <number variable="volume"/>
        </group>
      </else-if>
      
      <else>
        <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="technical-report">
    <choose>
      <if variable="number issue" match="any">
        <group delimiter=" ">
          
          <text value="Technical Report"/>
          <choose>
            <if variable="number">
              <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
            </if>
            <else>
              <text variable="issue" prefix="&lt;issue&gt;" suffix="&lt;/issue&gt;"/>
            </else>
          </choose>
        </group>
      </if>
      <else>
        
        <text value="Technical report"/>
      </else>
    </choose>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true">
    
    <layout prefix="(" suffix=")" delimiter="; ">
      <text macro="author-short"/>
      <text macro="year-date" prefix=", "/>
    </layout>
  </citation>
  <bibliography et-al-min="20" et-al-use-first="19">
    <sort>
      <key macro="author-or-editor"/>
      <key macro="year-date"/>
      <key variable="title"/>
    </sort>
    <layout>
      
      <choose>
        <if type="book">
          <text macro="author-or-editor" suffix="."/>
        </if>
        <else>
          <text macro="author" suffix="."/>
        </else>
      </choose>
      
      <date variable="issued" prefix=" " suffix=".">
        <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
      </date>
      
      <choose>
        
        <if type="article-journal">
          <text variable="title" prefix="&lt;title&gt; " suffix=".&lt;/title&gt;"/>
          <group prefix=" " suffix=".">
            <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <text variable="volume" prefix="&lt;volume&gt;, " suffix="&lt;/volume&gt;"/>
            <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
            <text variable="page" prefix="&lt;page&gt;:" suffix="&lt;/page&gt;"/>
            <text macro="month" prefix=", "/>
          </group>
        </if>
        
        <else-if type="book">
          <text variable="title" prefix="&lt;title&gt; " suffix=".&lt;/title&gt;" font-style="italic"/>
          <group delimiter=", " suffix=".">
            <text macro="volume-and-collection-title"/>
          </group>
          <group delimiter=", " prefix=" " suffix=".">
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            <text macro="edition"/>
            <text macro="month"/>
          </group>
        </else-if>
        
        <else-if type="chapter">
          <text variable="title" prefix="&lt;title&gt; " suffix=".&lt;/title&gt;"/>
          <text term="in" text-case="capitalize-first" prefix=" " suffix=" "/>
          <group delimiter=", " suffix=".">
            <text macro="editor"/>
            <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <text macro="volume-and-collection-title"/>
            <group delimiter=" " prefix="&lt;chapter-number&gt;" suffix="&lt;/chapter-number&gt;">
              <text term="chapter"/>
              <number variable="chapter-number"/>
            </group>
            <group delimiter=" ">
              <label variable="page"/>
              <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
            </group>
          </group>
          <group delimiter=", " prefix=" " suffix=".">
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            <text macro="edition"/>
            <text macro="month"/>
          </group>
        </else-if>
        
        <else-if type="pamphlet">
          <text variable="title" prefix="&lt;title&gt; " suffix=".&lt;/title&gt;"/>
          <group delimiter=", " prefix=" " suffix=".">
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            <text macro="month"/>
          </group>
        </else-if>
        
        <else-if type="paper-conference">
          <text variable="title" prefix="&lt;title&gt; " suffix=". &lt;/title&gt;"/>
          <text term="in" text-case="capitalize-first" suffix=" "/>
          <group suffix="." delimiter=", ">
            <text macro="editor"/>
            <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <text macro="volume-or-number"/>
            <group delimiter=" ">
              <label variable="page"/>
              <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
            </group>
            <choose>
              <if variable="event-place publisher-place" match="any">
                <text macro="event-or-publisher-place"/>
                <text macro="month"/>
              </if>
            </choose>
          </group>
          <group prefix=" " suffix="." delimiter=", ">
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
            <choose>
              <if variable="event-place publisher-place" match="none">
                <text macro="month"/>
              </if>
            </choose>
          </group>
        </else-if>
        
        <else-if type="report">
          <text variable="title" prefix="&lt;title&gt; " suffix=".&lt;/title&gt;"/>
          <group delimiter=", " prefix=" " suffix=".">
            <text macro="technical-report"/>
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            <text macro="month"/>
          </group>
        </else-if>
        
        <else-if type="thesis">
          <text variable="title" prefix="&lt;title&gt; " suffix=".&lt;/title&gt;" font-style="italic"/>
          <group delimiter=", " prefix=" " suffix=".">
            
            <text macro="thesis-type"/>
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            <text macro="month"/>
          </group>
        </else-if>
        
        <else>
          <text variable="title" prefix="&lt;title&gt; " suffix=".&lt;/title&gt;"/>
          <text macro="month" prefix=" " suffix="."/>
        </else>
      </choose>
      
      <text variable="note" prefix="&lt;note&gt; " suffix=".&lt;/note&gt;"/>
    </layout>
  </bibliography>
</style>