<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" version="1.0" page-range-format="minimal-two" demote-non-dropping-particle="never" default-locale="en-GB">
  <info>
    <title>Bioethics</title>
    <id>http://www.zotero.org/styles/bioethics</id>
    <link href="http://www.zotero.org/styles/bioethics" rel="self"/>
    <link href="http://www.zotero.org/styles/journal-of-roman-archaeology-b" rel="template"/>
    <link href="http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1467-8519/homepage/ForAuthors.html" rel="documentation"/>
    <author>
      <name>Patrick O'Brien</name>
    </author>
    <category citation-format="note"/>
    <category field="anthropology"/>
    <issn>0269-9702</issn>
    <eissn>1467-8519</eissn>
    <updated>2017-07-12T11:26:44+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name sort-separator=", " initialize-with="." delimiter=", "/>
      <label form="short" prefix=", "/>
      <substitute>
        <text macro="container-author"/>
      </substitute>
    </names>
  </macro>
  <macro name="container-author">
    <names variable="container-author" delimiter=", " prefix="&lt;container-author&gt;" suffix="&lt;/container-author&gt;">
      <name sort-separator=", " initialize-with="." delimiter=", "/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name initialize-with="."/>
      <substitute>
        <text macro="editor"/>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor-short">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name form="short" sort-separator=", " delimiter=", "/>
    </names>
  </macro>
  <macro name="container-author-short">
    <names variable="container-author" delimiter=", " prefix="&lt;container-author&gt;" suffix="&lt;/container-author&gt;">
      <name form="short" sort-separator=", " delimiter=", "/>
      <et-al font-style="italic"/>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" sort-separator=", " delimiter=", "/>
      <et-al font-style="italic"/>
      <substitute>
        <text macro="editor-short"/>
        <text macro="container-author-short"/>
        <text variable="title-short" font-style="italic" prefix="&lt;title-short&gt;" suffix="&lt;/title-short&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="collection">
    <group delimiter=" " suffix=", ">
      <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
      <text variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
    </group>
  </macro>
  <macro name="encyclopedia">
    <group delimiter=" ">
      <choose>
        <if variable="title-short" match="any">
          <text variable="title" form="short" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        </if>
        <else>
          <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
        </else>
      </choose>
      <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
      <date variable="issued" prefix="(" suffix=")">
        <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
      </date>
      <text variable="page" suffix=",&lt;/page&gt;" prefix="&lt;page&gt;"/>
      <text variable="title" prefix="&lt;title&gt;s.v. " quotes="true" suffix="&lt;/title&gt;"/>
      <text macro="author" prefix="(" suffix=")"/>
    </group>
  </macro>
  <macro name="title">
    <choose>
      <if type="book thesis" match="any">
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else-if type="webpage post-weblog" match="any">
        <date form="text" variable="issued" suffix=". &lt;/issued&gt;" prefix="&lt;issued&gt;"/>
        <text variable="title" font-style="italic" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
        <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </else-if>
      <else-if type="article-journal article-magazine article-newspaper" match="any">
        <group delimiter=". ">
          <text variable="title" quotes="false" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          <group delimiter=" ">
            <text variable="container-title" form="short" strip-periods="true" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <group delimiter="; ">
              <text macro="year-date"/>
              <group delimiter=": ">
                <group>
                  <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                  <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
                </group>
                <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
              </group>
            </group>
          </group>
        </group>
      </else-if>
      <else-if type="chapter paper-conference" match="any">
        <group delimiter=". " suffix=". ">
          <text macro="year-date"/>
          <text variable="title" quotes="false" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        </group>
        <group delimiter=". ">
          <group delimiter=" ">
            <text term="in" text-case="capitalize-first"/>
            <text macro="editor"/>
          </group>
          <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
        </group>
      </else-if>
      <else>
        <text variable="title" font-style="italic" font-weight="bold" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <choose>
      <if variable="collection-title" match="any">
        <text macro="collection"/>
      </if>
      <else-if type="thesis">
        <text variable="genre" suffix=",&lt;/genre&gt;" prefix="&lt;genre&gt;"/>
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </else-if>
      <else>
        <group delimiter=": ">
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
          <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short"/>
      </else>
    </choose>
  </macro>
  <macro name="locator">
    <choose>
      <if locator="page">
        <text variable="locator" prefix="&lt;locator&gt;: " suffix="&lt;/locator&gt;"/>
      </if>
      <else-if locator="paragraph">
        <text variable="locator" prefix="&lt;locator&gt;§" suffix="&lt;/locator&gt;"/>
      </else-if>
      <else-if type="webpage post-weblog" match="any">
        <group delimiter=" " prefix=". ">
          <group delimiter=": ">
            <text term="available at" text-case="capitalize-first"/>
            <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
          </group>
          <group delimiter=" " prefix="[" suffix="]">
            <text term="accessed" text-case="capitalize-first"/>
            <date form="text" variable="accessed" prefix="&lt;accessed&gt;" suffix="&lt;/accessed&gt;"/>
          </group>
        </group>
      </else-if>
      <else>
        <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
      </else>
    </choose>
  </macro>
  <citation and="symbol" delimiter-precedes-et-al="always" delimiter-precedes-last="always" et-al-min="3" et-al-use-first="1">
    <layout delimiter="; " suffix=".">
      <choose>
        <if type="entry-dictionary entry-encyclopedia" match="any">
          <text macro="encyclopedia"/>
        </if>
        <else-if position="ibid-with-locator">
          <group delimiter=" ">
            <text term="ibid" suffix="."/>
            <text macro="locator"/>
          </group>
        </else-if>
        <else-if position="ibid">
          <text term="ibid" suffix="."/>
        </else-if>
        <else-if position="subsequent">
          <group delimiter=" ">
            <text macro="author-short"/>
            <group prefix="(" suffix=")" delimiter=" ">
              <text term="cited"/>
              <group delimiter=" ">
                <text term="note" form="short"/>
                <text variable="first-reference-note-number" prefix="&lt;first-reference-note-number&gt;" suffix="&lt;/first-reference-note-number&gt;"/>
              </group>
            </group>
            <text macro="locator"/>
          </group>
        </else-if>
        <else>
          <text macro="author" suffix=". "/>
          <text macro="title"/>
          <text macro="publisher" prefix=". "/>
          <text macro="locator"/>
        </else>
      </choose>
    </layout>
  </citation>
</style>