    <summary>Estilo de citas de la Escuela Nacional de Antropología e Historia -- variante nota corta</summary>
    <updated>2016-03-08T14:30:44-06:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  
  <macro name="secondary-contributors">
    <choose>
      <if type="chapter paper-conference article" match="any">
        <names variable="editor" delimiter=". " prefix="&lt;editor&gt;, " suffix="&lt;/editor&gt;">
          <name and="text" delimiter=", " delimiter-precedes-last="never"/>
          <label form="short" text-case="lowercase" prefix=" (" suffix=")" plural="contextual"/>
        </names>
      </if>
    </choose>
    <choose>
      <if type="chapter paper-conference" match="none">
        <names variable="translator" delimiter=". " prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;">
          <label form="verb" text-case="capitalize-first" prefix=". " suffix=" "/>
          <name and="text" delimiter=", "/>
        </names>
      </if>
    </choose>
  </macro>
  
  <macro name="director">
    <choose>
      <if type="motion_picture">
        <text term="director" form="short" prefix=" (" suffix=")"/>
      </if>
    </choose>
  </macro>
  
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name and="text" name-as-sort-order="first" sort-separator=", " delimiter=", " delimiter-precedes-last="never" font-variant="small-caps"/>
      <label form="short" plural="contextual" prefix=" (" suffix=")"/>
      <et-al font-style="italic" font-variant="normal"/>
    </names>
  </macro>
  
  <macro name="translator">
    <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;">
      <name and="text" name-as-sort-order="first" sort-separator=", " delimiter=", " delimiter-precedes-last="never" font-variant="small-caps"/>
      <label form="short" plural="contextual" prefix=" (" suffix=")"/>
      <et-al font-style="italic" font-variant="normal"/>
    </names>
  </macro>
  
  <macro name="anon">
    <text term="anonymous" form="long" text-case="capitalize-first"/>
  </macro>
  
  <macro name="contributors">
    <group delimiter=" ">
      <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
        <name and="text" name-as-sort-order="first" sort-separator=", " delimiter=", " delimiter-precedes-last="never" font-variant="small-caps"/>
        <label form="short" plural="contextual" prefix=", "/>
        <et-al font-style="italic" font-variant="normal"/>
        <substitute>
          <text macro="editor"/>
          <text macro="translator"/>
          <text macro="anon" font-variant="small-caps"/>
        </substitute>
      </names>
      <text macro="director"/>
    </group>
  </macro>
  
  <macro name="contributors-note">
    <group delimiter=" ">
      <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
        <name form="long" and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="always"/>
        <label form="short" plural="never" prefix=", "/>
        <et-al font-style="italic"/>
        <substitute>
          <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
            <name and="text" sort-separator=", " delimiter=", " delimiter-precedes-last="never"/>
            <label form="short" plural="contextual" prefix=" (" suffix=")"/>
            <et-al font-style="italic"/>
          </names>
          <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;">
            <name and="text" name-as-sort-order="first" sort-separator=", " delimiter=", " delimiter-precedes-last="never"/>
            <label form="short" plural="contextual" prefix=" (" suffix=")"/>
            <et-al font-style="italic"/>
          </names>
          <text macro="anon"/>
        </substitute>
      </names>
      <text macro="director"/>
    </group>
  </macro>
  
  <macro name="interviewer">
    <names variable="interviewer" delimiter=", " prefix="&lt;interviewer&gt;" suffix="&lt;/interviewer&gt;">
      <label form="verb" prefix=" " text-case="capitalize-first" suffix=" "/>
      <name and="text" delimiter=", "/>
    </names>
    <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
  </macro>
  
  <macro name="access">
    <group delimiter="">
      <choose>
        <if variable="URL">
          <text variable="URL" prefix="&lt;URL&gt;&lt;" suffix="&gt;&lt;/URL&gt;"/>
          <group>
            <text term="accessed" text-case="capitalize-first" prefix=". " suffix=" "/>
            <date variable="accessed" form="text">
              <date-part name="day" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
              <date-part name="month" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
              <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
            </date>
          </group>
        </if>
      </choose>
      <choose>
        <if variable="DOI">
          <text variable="DOI" prefix="&lt;DOI&gt;doi: " suffix="&lt;/DOI&gt;"/>
        </if>
      </choose>
    </group>
  </macro>
  
  <macro name="title">
    <choose>
      <if variable="title" match="none">
        <choose>
          <if type="personal_communication" match="none">
            <text variable="genre" text-case="capitalize-first" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
          </if>
        </choose>
      </if>
      <else-if type="bill book graphic legal_case thesis legislation motion_picture musical_score" match="any">
        <text variable="title" text-case="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else>
        <text variable="title" text-case="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  
  <macro name="title-short">
    <choose>
      <if variable="title" match="none">
        <choose>
          <if type="interview">
            <text term="interview"/>
          </if>
          <else-if type="manuscript speech" match="any">
            <text variable="genre" form="short" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
          </else-if>
          <else-if type="personal_communication">
            <text macro="issue"/>
          </else-if>
        </choose>
      </if>
      <else-if type="bill book graphic legislation motion_picture song" match="any">
        <text variable="title" text-case="title" form="short" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else-if type="legal_case interview" match="any">
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else>
        <text variable="title" text-case="title" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  
  <macro name="edition">
    <choose>
      <if type="bill book graphic legal_case legislation motion_picture report musical_score" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" " prefix="&lt;edition&gt;. " suffix="&lt;/edition&gt;">
              <number variable="edition" form="ordinal"/>
              <text term="edition" form="short" strip-periods="true"/>
            </group>
          </if>
          <else>
            <text variable="edition" prefix="&lt;edition&gt;. " suffix="&lt;/edition&gt;"/>
          </else>
        </choose>
      </if>
      <else-if type="chapter  paper-conference" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" " prefix="&lt;edition&gt;. " suffix="&lt;/edition&gt;">
              <number variable="edition" form="ordinal"/>
              <text term="edition" form="short"/>
            </group>
          </if>
          <else>
            <text variable="edition" prefix="&lt;edition&gt;, " suffix="&lt;/edition&gt;"/>
          </else>
        </choose>
      </else-if>
    </choose>
  </macro>
  
  <macro name="locators-title">
    <choose>
      <if type="bill book chapter graphic legal_case legislation motion_picture report musical_score" match="any">
        <group prefix=", " delimiter=". ">
          <group prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;">
            <text term="volume" form="short" text-case="lowercase" suffix=" "/>
            <number variable="volume" form="numeric"/>
          </group>
          <group prefix="&lt;number-of-volumes&gt;" suffix="&lt;/number-of-volumes&gt;">
            <number variable="number-of-volumes" form="numeric"/>
            <text term="volume" form="short" prefix=" " plural="true"/>
          </group>
        </group>
      </if>
      <else-if type="legal_case">
        <text variable="volume" prefix="&lt;volume&gt;, " suffix="&lt;/volume&gt;"/>
        <text variable="container-title" prefix="&lt;container-title&gt; " suffix="&lt;/container-title&gt;"/>
        <text variable="page" prefix="&lt;page&gt; " suffix="&lt;/page&gt;"/>
      </else-if>
      <else-if type="article-journal">
        <choose>
          <if variable="volume">
            <text variable="volume" prefix="&lt;volume&gt;, " suffix="&lt;/volume&gt;"/>
            <text variable="issue" prefix="&lt;issue&gt; (" suffix=")&lt;/issue&gt;"/>
          </if>
          <else>
            <text variable="issue" prefix="&lt;issue&gt; (" suffix=")&lt;/issue&gt;"/>
          </else>
        </choose>
      </else-if>
      <else-if type="chapter paper-conference" match="any">
        <choose>
          <if variable="page" match="none">
            <group prefix="&lt;volume&gt;, " suffix="&lt;/volume&gt;">
              <text term="volume" form="short" text-case="lowercase" suffix=" "/>
              <number variable="volume" form="numeric"/>
            </group>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  
  <macro name="locators">
    <choose>
      <if type="chapter paper-conference article-journal" match="any">
        <choose>
          <if variable="page">
            <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
          </if>
          <else>
            <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
          </else>
        </choose>
      </if>
    </choose>
    <choose>
      <if type="article-newspaper">
        <group prefix=", " delimiter=", ">
          <group>
            <text variable="edition" suffix=", &lt;/edition&gt;" prefix="&lt;edition&gt;"/>
            <text term="edition" prefix=" "/>
          </group>
          <group>
            <text variable="section" form="short" suffix=" &lt;/section&gt;" prefix="&lt;section&gt;"/>
            <text term="section"/>
          </group>
        </group>
        <group>
          <text variable="page" prefix="&lt;page&gt;: " suffix=". &lt;/page&gt;"/>
        </group>
      </if>
    </choose>
    <choose>
      <if type="webpage">
        <group prefix=", " delimiter=", ">
          <group>
            <text variable="edition" suffix=" &lt;/edition&gt;" prefix="&lt;edition&gt;"/>
            <text term="edition" prefix=" "/>
          </group>
          <group>
            <text variable="section" form="short" suffix=" &lt;/section&gt;" prefix="&lt;section&gt;"/>
            <text term="section"/>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  
  <macro name="container-title">
    <choose>
      <if type="chapter paper-conference" match="any">
        <text term="in" text-case="lowercase" prefix=", " suffix=" "/>
      </if>
    </choose>
    <choose>
      <if type="paper-conference">
        <text variable="container-title" text-case="title" font-style="normal" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </if>
      <else-if type="chapter">
        <text variable="container-title" text-case="title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </else-if>
      <else-if type="legal_case" match="none">
        <text variable="container-title" text-case="title" font-style="italic" prefix="&lt;container-title&gt;. " suffix="&lt;/container-title&gt;"/>
      </else-if>
    </choose>
  </macro>
  
  <macro name="publisher">
    <group>
      <choose>
        <if variable="collection-title">
          <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          <text macro="collection-title" prefix=" " suffix=". "/>
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
        </if>
        <else-if variable="publisher-place">
          <text variable="publisher" suffix=". &lt;/publisher&gt;" prefix="&lt;publisher&gt;"/>
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
        </else-if>
        <else>
          <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
        </else>
      </choose>
    </group>
  </macro>
  
  <macro name="date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else-if variable="accessed">
        <date variable="accessed">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </else-if>
      <else>
        <text term="no date" form="short" text-case="lowercase"/>
      </else>
    </choose>
  </macro>
  
  <macro name="day-month">
    <date variable="issued">
      <date-part name="day" suffix=" de &lt;/day&gt;" prefix="&lt;day&gt;"/>
      <date-part name="month" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
    </date>
  </macro>
  
  <macro name="collection-title">
    <text variable="collection-title" text-case="title" prefix="&lt;collection-title&gt;(" suffix=")&lt;/collection-title&gt;"/>
    <text variable="collection-number" prefix="&lt;collection-number&gt;( " suffix=")&lt;/collection-number&gt;"/>
  </macro>
  
  <macro name="event">
    <group>
      <text term="presented at" suffix=" "/>
      <text variable="event" prefix="&lt;event&gt;" suffix="&lt;/event&gt;"/>
    </group>
  </macro>
  
  <macro name="description">
    <choose>
      <if type="interview">
        <group delimiter=". ">
          <text macro="interviewer"/>
          <text variable="genre" text-case="capitalize-first" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
        </group>
      </if>
    </choose>
    <choose>
      <if type="thesis">
        <group delimiter=" ">
          <text value="Tesis de" text-case="lowercase" prefix=", "/>
          <text variable="genre" suffix=".&lt;/genre&gt;" prefix="&lt;genre&gt;"/>
        </group>
      </if>
    </choose>
    <choose>
      <if type="report">
        <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
      </if>
    </choose>
    <group delimiter=" " prefix=" (" suffix=")">
      <text term="version"/>
      <text variable="version" prefix="&lt;version&gt;" suffix="&lt;/version&gt;"/>
    </group>
  </macro>
  
  <macro name="media">
    <choose>
      <if variable="genre">
        <choose>
          <if type="article-journal article-newspaper book chapter interview" match="any">
            <text variable="genre" font-variant="small-caps" prefix="&lt;genre&gt; [" suffix="]&lt;/genre&gt;"/>
          </if>
        </choose>
        <choose>
          <if type="article" match="any">
            <text variable="genre" font-variant="small-caps" prefix="&lt;genre&gt; [" suffix="]&lt;/genre&gt;"/>
          </if>
        </choose>
        <choose>
          <if type="motion_picture musical_score" match="any">
            <choose>
              <if variable="page">
                <text variable="genre" font-variant="small-caps" prefix="&lt;genre&gt; [" suffix="&lt;/genre&gt;"/>
                <text variable="page" prefix="&lt;page&gt;, " suffix=" mins.]&lt;/page&gt;"/>
              </if>
              <else>
                <text variable="genre" font-variant="small-caps" prefix="&lt;genre&gt; [" suffix="]&lt;/genre&gt;"/>
              </else>
            </choose>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  
  <macro name="issue">
    <choose>
      <if type="article-journal">
        <text macro="day-month" prefix=", " suffix=""/>
      </if>
      <else-if type="legal_case">
        <text variable="authority" prefix="&lt;authority&gt;. " suffix="&lt;/authority&gt;"/>
      </else-if>
      <else-if type="speech">
        <group prefix=" " delimiter=", ">
          <text macro="event"/>
          <text macro="day-month"/>
          <text variable="event-place" prefix="&lt;event-place&gt;" suffix="&lt;/event-place&gt;"/>
        </group>
      </else-if>
      <else-if type="article-newspaper article-magazine personal_communication" match="any">
        <text macro="day-month" prefix=", "/>
      </else-if>
      <else>
        <group prefix=". " delimiter=", ">
          <text macro="publisher"/>
        </group>
      </else>
    </choose>
  </macro>
  
  <citation et-al-min="2" et-al-use-first="1" disambiguate-add-year-suffix="true" after-collapse-delimiter=";">
    <layout suffix="." delimiter="; ">
      <choose>
        <if position="ibid-with-locator">
          <group delimiter="">
            <text term="ibid" font-style="italic" text-case="capitalize-first"/>
            <label variable="locator" form="short" prefix=", "/>
            <text variable="locator" prefix="&lt;locator&gt; " suffix="&lt;/locator&gt;"/>
          </group>
        </if>
        <else-if position="ibid">
          <text value="id." font-style="italic" text-case="capitalize-first"/>
        </else-if>
        <else-if position="subsequent">
          <group delimiter="">
            <text macro="contributors-note"/>
            <choose>
              <if disambiguate="true">
                <text macro="title-short" prefix=". "/>
                <choose>
                  <if type="article-journal article-magazine article-newspaper" match="none">
                    <text macro="locators-title" suffix=""/>
                  </if>
                </choose>
                <choose>
                  <if type="article-journal article-magazine article-newspaper" match="any">
                    <text value="art. cit." prefix=", "/>
                  </if>
                  <else>
                    <text value="op. cit." font-style="italic" prefix=", "/>
                  </else>
                </choose>
              </if>
              <else>
                <choose>
                  <if type="article-journal article-magazine article-newspaper" match="any">
                    <text value="art. cit." prefix=", "/>
                  </if>
                  <else>
                    <text value="op. cit." font-style="italic" prefix=", "/>
                  </else>
                </choose>
              </else>
            </choose>
            <group>
              <label variable="locator" form="short" prefix=", " suffix=" "/>
              <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
            </group>
          </group>
        </else-if>
        <else>
          <group>
            <text macro="contributors-note" suffix=". "/>
            <text macro="title"/>
            <text macro="description"/>
            <text macro="container-title"/>
            <choose>
              <if type="article-journal article-magazine article-newspaper" match="none">
                <text macro="locators-title"/>
              </if>
            </choose>
            <group>
              <text value="véase" prefix=", "/>
              <label variable="locator" form="short" plural="contextual" prefix=" " suffix=" "/>
              <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
            </group>
          </group>
        </else>
      </choose>
    </layout>
  </citation>
  
  <bibliography et-al-min="4" et-al-use-first="3" subsequent-author-substitute="" entry-spacing="0">
    <sort>
      <key macro="contributors"/>
      <key macro="date"/>
      <key macro="locators-title"/>
    </sort>
    <layout suffix=".">
      <group display="block">
        <text macro="contributors"/>
      </group>
      <group display="left-margin">
        <text macro="date" suffix=" "/>
        <date variable="original-date" prefix="[" suffix="]">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </group>
      <group display="right-inline">
        <group>
          <text macro="title"/>
          <text macro="description"/>
          <text macro="container-title"/>
          <text macro="locators-title"/>
          <text macro="secondary-contributors"/>
          <text macro="edition"/>
          <text macro="issue"/>
          <text macro="locators"/>
          <text macro="access" prefix=". "/>
          <text macro="media"/>
        </group>
      </group>
    </layout>
  </bibliography>
</style>