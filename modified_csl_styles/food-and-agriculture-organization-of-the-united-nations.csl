<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only">
  
  <info>
    <title>Food and Agriculture Organization of the United Nations</title>
    <title-short>FAO</title-short>
    <id>http://www.zotero.org/styles/food-and-agriculture-organization-of-the-united-nations</id>
    <link href="http://www.zotero.org/styles/food-and-agriculture-organization-of-the-united-nations" rel="self"/>
    <link href="http://www.fao.org/3/a-ac339e.pdf" rel="documentation"/>
    <author>
      <name>Bin Liu</name>
      <email>lieubean@gmail.com</email>
      <uri>https://www.linkedin.com/in/lieubean/</uri>
    </author>
    <contributor>
      <name>Julian Plummer</name>
      <email>Julian.Plummer@fao.org</email>
    </contributor>
    <category citation-format="author-date"/>
    <category field="science"/>
    <category field="social_science"/>
    <summary>This style is created to meet the citation and bibliographical requirements of FAOSTYLE, and has been tested with Zotero and Mendeley. Last update: March 2017.</summary>
    <updated>2017-03-10T01:12:03+01:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  
  <locale xml:lang="en">
    <style-options punctuation-in-quote="false"/>
    <terms>
      <term name="no date">undated</term>
      <term name="available at">also available at</term>
      <term name="presented at">Paper presented at</term>
    </terms>
    <date delimiter=" " form="text">
      <date-part name="day"/>
      <date-part name="month"/>
      <date-part name="year"/>
    </date>
  </locale>
  
  <macro name="anonymous">
    <text term="anonymous" text-case="capitalize-first"/>
  </macro>
  <macro name="editor">
    
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name and="symbol" delimiter-precedes-last="never" initialize-with="."/>
      <label form="short" strip-periods="true" prefix=", "/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name and="symbol" delimiter-precedes-last="never" initialize-with="." name-as-sort-order="all"/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <text macro="anonymous"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="text" delimiter-precedes-et-al="never" delimiter-precedes-last="never" initialize-with="."/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <choose>
          
          <if match="none" type="legal_case">
            <text macro="anonymous"/>
          </if>
          <else>
            <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="date">
    
    <choose>
      <if match="none" variable="issued">
        <text term="no date"/>
      </if>
      <else>
        <date date-parts="year" form="text" variable="issued" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher-and-place">
    
    <group delimiter=", ">
      <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
      <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
    </group>
  </macro>
  <macro name="edition">
    
    <number text-case="capitalize-first" variable="edition" form="long-ordinal" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
    <text term="edition" prefix=" "/>
  </macro>
  <macro name="Cite-and-URL">
    
    <group delimiter=" " prefix="[" suffix="]. ">
      <text term="cited" text-case="capitalize-first"/>
      <date form="text" variable="accessed" prefix="&lt;accessed&gt;" suffix="&lt;/accessed&gt;"/>
    </group>
    <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
  </macro>
  <macro name="Available-at">
    
    <group delimiter=" " prefix="(" suffix=").">
      <text term="available at"/>
      <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
    </group>
  </macro>
  <macro name="number_of_pages-label">
    
    <group delimiter=" ">
      <text variable="number-of-pages" prefix="&lt;number-of-pages&gt;" suffix="&lt;/number-of-pages&gt;"/>
      <label variable="number-of-pages" form="short"/>
    </group>
  </macro>
  
  <citation disambiguate-add-year-suffix="true" collapse="year" et-al-min="4" et-al-use-first="1">
    <sort>
      <key variable="author"/>
      <key variable="issued"/>
      <key variable="accessed"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <text macro="author-short" suffix=", "/>
      <text macro="date"/>
    </layout>
  </citation>
  
  <bibliography entry-spacing="0">
    <sort>
      <key macro="author"/>
      <key variable="issued"/>
      <key variable="accessed"/>
    </sort>
    <layout>
      <choose>
        <if match="none" type="legal_case">
          
          <group delimiter=". ">
            <text macro="author" font-weight="bold"/>
            <text macro="date"/>
            <choose>
              <if type="book">
                
                <group>
                  <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                  <text variable="medium" prefix="&lt;medium&gt; [" suffix="]&lt;/medium&gt;"/>
                  
                  <text variable="genre" prefix="&lt;genre&gt; [" suffix="]&lt;/genre&gt;"/>
                  
                  <text macro="edition" prefix=". "/>
                </group>
                <text macro="editor"/>
                <group>
                  <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                  <text variable="collection-number" prefix="&lt;collection-number&gt; No. " suffix="&lt;/collection-number&gt;"/>
                  
                </group>
                <text macro="publisher-and-place" suffix="."/>
                <text macro="number_of_pages-label"/>
                <text macro="Available-at"/>
              </if>
              <else-if type="chapter report" match="any">
                
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <group delimiter=" ">
                  <text term="in" text-case="capitalize-first" font-style="italic"/>
                  <text macro="editor"/>
                </group>
                <group>
                  <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                  <text macro="edition" prefix=". "/>
                  <label variable="page" prefix=", " form="short"/>
                  
                  <text variable="page" prefix="&lt;page&gt; " suffix="&lt;/page&gt;"/>
                </group>
                <group>
                  <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                  <text variable="collection-number" prefix="&lt;collection-number&gt; No. " suffix="&lt;/collection-number&gt;"/>
                  
                  <text variable="number" prefix="&lt;number&gt; No. " suffix="&lt;/number&gt;"/>
                  
                  
                </group>
                <group suffix=".">
                  <text macro="publisher-and-place"/>
                  <text macro="number_of_pages-label"/>
                </group>
                <text macro="Available-at"/>
              </else-if>
              <else-if type="article-journal">
                
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <group suffix=".">
                  <text variable="container-title" form="long" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                  <text variable="volume" prefix="&lt;volume&gt;, " suffix="&lt;/volume&gt;"/>
                  <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
                  <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
                </group>
                <choose>
                  
                  <if variable="DOI">
                    <text variable="DOI" prefix="&lt;DOI&gt;https://doi.org/" suffix="&lt;/DOI&gt;"/>
                  </if>
                  <else>
                    <text macro="Available-at"/>
                  </else>
                </choose>
              </else-if>
              <else-if type="article-magazine">
                
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <group suffix=".">
                  <text variable="container-title" form="long" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                  <text variable="volume" prefix="&lt;volume&gt;, " suffix="&lt;/volume&gt;"/>
                  <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
                  <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
                  <choose>
                    <if variable="URL">
                      
                      <text term="online" prefix=" [" suffix="]"/>
                    </if>
                  </choose>
                </group>
                <text macro="Cite-and-URL"/>
              </else-if>
              <else-if type="article-newspaper">
                
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <group>
                  <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                  <date form="text" variable="issued" prefix="&lt;issued&gt;, " suffix="&lt;/issued&gt;"/>
                </group>
                <text macro="Available-at"/>
              </else-if>
              <else-if type="webpage post-weblog" match="any">
                
                <choose>
                  <if variable="container-title">
                    <group>
                      <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                      <text term="in" text-case="capitalize-first" prefix=". "/>
                      <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;: " suffix="&lt;/container-title&gt;"/>
                      <text term="online" prefix=" [" suffix="]"/>
                    </group>
                  </if>
                  <else>
                    <group>
                      <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                      <text term="online" prefix=" [" suffix="]"/>
                    </group>
                  </else>
                </choose>
                <text variable="note" prefix="&lt;note&gt;" suffix="&lt;/note&gt;"/>
                
                <text macro="Cite-and-URL"/>
              </else-if>
              <else-if type="thesis">
                
                <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <text macro="publisher-and-place"/>
                <text variable="genre" prefix="&lt;genre&gt; (" suffix=")&lt;/genre&gt;"/>
                <text macro="Available-at"/>
              </else-if>
              <else-if type="song motion_picture article" match="any">
                
                <choose>
                  <if match="any" variable="medium genre">
                    <group>
                      <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                      <text variable="medium" prefix="&lt;medium&gt; [" suffix="]&lt;/medium&gt;"/>
                      
                      <text variable="genre" prefix="&lt;genre&gt; [" suffix="]&lt;/genre&gt;"/>
                      
                    </group>
                  </if>
                  <else>
                    <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                  </else>
                </choose>
                <text macro="publisher-and-place" suffix="."/>
                <text macro="Cite-and-URL"/>
              </else-if>
              <else-if type="manuscript">
                
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <text variable="genre" suffix=".&lt;/genre&gt;" prefix="&lt;genre&gt;"/>
                <text variable="publisher-place" suffix=".&lt;/publisher-place&gt;" prefix="&lt;publisher-place&gt;"/>
                <text macro="number_of_pages-label"/>
                <text macro="Available-at"/>
              </else-if>
              <else-if type="speech" match="any">
                
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <group suffix=".">
                  <text term="presented at"/>
                  <text variable="event" prefix="&lt;event&gt; " suffix="&lt;/event&gt;"/>
                  <date form="text" variable="issued" prefix="&lt;issued&gt;, " suffix="&lt;/issued&gt;"/>
                  <text variable="publisher-place" prefix="&lt;publisher-place&gt;, " suffix="&lt;/publisher-place&gt;"/>
                </group>
                <text macro="Cite-and-URL"/>
              </else-if>
              <else-if type="paper-conference">
                
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                <group>
                  <text term="in" text-case="capitalize-first" font-style="italic"/>
                  <text macro="editor" prefix=" "/>
                </group>
                <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                <group suffix=".">
                  <label suffix=" " variable="page" form="short"/>
                  <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
                </group>
                <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                <group suffix=".">
                  <text term="presented at"/>
                  <text variable="event" prefix="&lt;event&gt; " suffix="&lt;/event&gt;"/>
                  <date form="text" variable="issued" prefix="&lt;issued&gt;, " suffix="&lt;/issued&gt;"/>
                  <text variable="publisher-place" prefix="&lt;publisher-place&gt;, " suffix="&lt;/publisher-place&gt;"/>
                </group>
                <text macro="Available-at"/>
              </else-if>
              <else>
                
                <text variable="title" suffix=".&lt;/title&gt;" prefix="&lt;title&gt;"/>
                <text macro="publisher-and-place" suffix="."/>
                <text macro="Cite-and-URL"/>
              </else>
            </choose>
          </group>
        </if>
        <else>
          
          <group delimiter=" ">
            <group>
              <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
              <text variable="volume" prefix="&lt;volume&gt;, " suffix="&lt;/volume&gt;"/>
            </group>
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
            <group delimiter=" " prefix="(" suffix=")">
              <text variable="authority" prefix="&lt;authority&gt;" suffix="&lt;/authority&gt;"/>
              
              <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
              
              <text macro="date"/>
            </group>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>