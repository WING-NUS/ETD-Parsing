<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" class="in-text" demote-non-dropping-particle="never" default-locale="en-GB" page-range-format="chicago">
  <info>
    <title>McDonald Institute Monographs</title>
    <id>http://www.zotero.org/styles/mcdonald-institute-monographs</id>
    <link href="http://www.zotero.org/styles/mcdonald-institute-monographs" rel="self"/>
    <link href="http://www.mcdonald.cam.ac.uk/publications/publication-images/mcd-instructions" rel="documentation"/>
    <author>
      <name>Nicolas Monteix</name>
      <email>nicolas.monteix@univ-rouen.fr</email>
    </author>
    <category citation-format="author-date"/>
    <category field="history"/>
    <category field="anthropology"/>
    <issn>1363-1349</issn>
    <summary>Harvard author-date style edited for McDonald Institute Monographs, published by the McDonald Institute for Archaeological Research</summary>
    <updated>2014-05-02T11:00:00+01:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names variable="editor" delimiter=", " prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <label form="short" prefix=" (" text-case="lowercase" suffix=")"/>
      <name and="symbol" name-as-sort-order="first" sort-separator=", " initialize-with="." delimiter=", " delimiter-precedes-last="never"/>
    </names>
  </macro>
  <macro name="editor-bib">
    <names variable="editor" delimiter=", " prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <label form="short" prefix=" " text-case="lowercase" suffix=" "/>
      <name initialize-with="." and="symbol" sort-separator=", " delimiter=", " delimiter-precedes-last="never"/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name and="symbol" name-as-sort-order="first" sort-separator=", " initialize-with="." delimiter=", " delimiter-precedes-last="never"/>
      <label form="short" prefix=" " text-case="lowercase"/>
      <substitute>
        <text macro="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor-short">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name form="short" and="symbol" delimiter=", " delimiter-precedes-last="never"/>
      <et-al font-style="italic"/>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="symbol" delimiter=", " delimiter-precedes-last="never"/>
      <et-al font-style="italic"/>
      <substitute>
        <text macro="editor-short"/>
        <text variable="title" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <group prefix=" [" suffix="]">
      <text term="accessed" text-case="capitalize-first" suffix=" "/>
      <date variable="accessed">
        <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
        <date-part name="month" form="short" strip-periods="true" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
        <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
      </date>
    </group>
  </macro>
  <macro name="collection">
    <group prefix=" (" suffix=")" delimiter=" ">
      <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
      <text variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
    </group>
  </macro>
  <macro name="title">
    <choose>
      <if type="book" match="any">
        <text variable="title" form="long" text-case="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else-if type="webpage" match="any">
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else-if variable="container-title" match="none">
        <text variable="title" text-case="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
    <text variable="publisher" prefix="&lt;publisher&gt;: " suffix="&lt;/publisher&gt;"/>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;, " suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="long" text-case="lowercase" strip-periods="true"/>
        </group>
      </if>
      <else>
        <text variable="edition" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <group>
      <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
    </group>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" collapse="year" cite-group-delimiter="; ">
    <sort>
      <key variable="author"/>
      <key variable="issued"/>
      <key variable="container-title"/>
      <key variable="page-first"/>
      <key variable="title"/>
    </sort>
    <layout delimiter="; " prefix="(" suffix=")">
      <group>
        <text macro="author-short" suffix=" "/>
        <text macro="year-date"/>
        <choose>
          <if locator="page">
            <text variable="locator" prefix="&lt;locator&gt;, " suffix="&lt;/locator&gt;"/>
          </if>
          <else-if locator="paragraph">
            <text variable="locator" prefix="&lt;locator&gt;, §" suffix="&lt;/locator&gt;"/>
          </else-if>
          <else>
            <group prefix=", ">
              <label variable="locator" form="short" suffix=". "/>
              <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
            </group>
          </else>
        </choose>
      </group>
    </layout>
  </citation>
  <bibliography>
    <sort>
      <key macro="author"/>
      <key variable="issued"/>
      <key variable="container-title"/>
      <key variable="page-first"/>
      <key variable="title"/>
    </sort>
    <layout>
      <choose>
        <if variable="author editor" match="any">
          <text macro="author"/>
        </if>
        <else>
          <text variable="title" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        </else>
      </choose>
      <text macro="year-date" prefix=", " suffix="."/>
      <choose>
        <if type="thesis">
          <group prefix=" " delimiter=", " suffix=".">
            <text macro="title" text-case="title" font-style="italic"/>
            <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          </group>
        </if>
        <else-if type="webpage">
          <group prefix=" " delimiter=" " suffix=".">
            <text macro="title" font-style="italic" suffix="."/>
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <date variable="issued">
              <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
              <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
              <date-part name="year" suffix=" &lt;/year&gt;" prefix="&lt;year&gt;"/>
            </date>
          </group>
          <group prefix=" " delimiter=" " suffix=".">
            <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
            <text macro="access"/>
          </group>
        </else-if>
        <else-if type="article-journal article-magazine article-newspaper broadcast personal_communication entry-dictionary entry-encyclopedia" match="any">
          <group suffix=".">
            <group>
              <text macro="title" prefix=" " suffix=","/>
              <text macro="editor"/>
              <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt; " suffix="&lt;/container-title&gt;"/>
            </group>
            <group prefix=" ">
              <group suffix=", ">
                <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
              </group>
              <group>
                <text macro="pages"/>
              </group>
            </group>
          </group>
        </else-if>
        <else-if type="book graphic" match="any">
          <group prefix=" " suffix=".">
            <text macro="title"/>
            <text macro="edition"/>
            <text macro="collection"/>
            <text macro="publisher" prefix=". "/>
          </group>
        </else-if>
        <else-if type="chapter paper-conference" match="any">
          <group prefix=" " suffix=".">
            <group suffix=", ">
              <text macro="title"/>
            </group>
            <group prefix=" " suffix=".">
              <text term="in" suffix=" "/>
              <text variable="container-title" text-case="title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
              <text macro="edition" prefix=". "/>
              <text macro="editor-bib" prefix=", "/>
              <text macro="collection"/>
              <text macro="publisher" prefix=". "/>
              <text macro="pages" prefix=", "/>
            </group>
          </group>
        </else-if>
      </choose>
    </layout>
  </bibliography>
</style>