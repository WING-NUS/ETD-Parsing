<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" class="note" default-locale="en-US">
  <info>
    <title>Canadian Guide to Uniform Legal Citation 7th edition (McGill Guide)</title>
    <id>http://www.zotero.org/styles/mcgill-en</id>
    <link href="http://www.zotero.org/styles/mcgill-en" rel="self"/>
    <link href="http://www.lawjournal.mcgill.ca/en/text/22" rel="documentation"/>
    <author>
      <name>Liam McHugh-Russell</name>
      <email>liam.mchugh.russell@gmail.com</email>
    </author>
    <contributor>
      <name>Frank Bennett</name>
    </contributor>
    <contributor>
      <name>Florian Martin-Bariteau</name>
      <email>f.martin-bariteau@umontreal.ca</email>
      <uri>http://f-mb.github.io/cslegal/</uri>
    </contributor>
    <category citation-format="note"/>
    <category field="law"/>
    <updated>2014-06-28T19:23:00+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <style-options punctuation-in-quote="false"/>
    <terms>
      <term name="et-al">et al</term>
      <term name="ordinal">th</term>
      <term name="ordinal-01">st</term>
      <term name="ordinal-02">d</term>
      <term name="ordinal-03">d</term>
      <term name="ordinal-11">th</term>
      <term name="ordinal-12">th</term>
      <term name="ordinal-13">th</term>
      <term name="paragraph" form="short">
        <single>para</single>
        <multiple>paras</multiple>
      </term>
      
      <term name="section" form="short">
        <single>s</single>
        <multiple>ss</multiple>
      </term>
      <term name="chapter" form="short">
        <single>ch</single>
        <multiple>chs</multiple>
      </term>
    </terms>
  </locale>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name and="symbol" delimiter-precedes-last="never"/>
      <et-al term="et-al"/>
      <label form="short" prefix=", "/>
    </names>
  </macro>
  <macro name="translator">
    <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;">
      <label form="verb" suffix=" "/>
      <name and="symbol"/>
    </names>
  </macro>
  <macro name="container-title">
    <text strip-periods="true" variable="container-title" form="short" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
  </macro>
  <macro name="author-note">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name and="symbol" delimiter-precedes-last="never"/>
      <et-al term="et-al"/>
      <substitute>
        <text macro="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-bib">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name name-as-sort-order="first" and="symbol" sort-separator=", " delimiter-precedes-last="never"/>
      <et-al term="et-al"/>
      <label form="short" prefix=", "/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="internet-location">
    <choose>
      <if variable="URL">
        <text term="online" prefix=", " suffix=": &lt;"/>
        <text variable="URL" suffix="&gt;&lt;/URL&gt;" prefix="&lt;URL&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="genre">
    <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
  </macro>
  <macro name="issued-long">
    <date variable="issued" delimiter=" ">
      <date-part name="day" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
      <date-part name="month" form="long" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
      <date-part name="year" form="long" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <macro name="edition">
    <number variable="edition" form="ordinal" text-case="lowercase" suffix=" &lt;/edition&gt;" prefix="&lt;edition&gt;"/>
    <text term="edition" form="short" strip-periods="true"/>
  </macro>
  <macro name="references">
    <text variable="references" prefix="&lt;references&gt;" suffix="&lt;/references&gt;"/>
  </macro>
  <macro name="series-info">
    <group delimiter=" ">
      <text variable="collection-title" strip-periods="true" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
      <text variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
    </group>
  </macro>
  <macro name="sort-by-type">
    <choose>
      <if type="bill legislation" match="any">
        <text value="1"/>
      </if>
      <else-if type="legal_case">
        <text value="2"/>
      </else-if>
      <else-if type="book thesis" match="any">
        <text value="3"/>
      </else-if>
      <else-if type="article-journal chapter article-newspaper article-magazine" match="any">
        <text value="4"/>
      </else-if>
      <else>
        <text value="5"/>
      </else>
    </choose>
  </macro>
  
  <macro name="render-chapter">
    <group delimiter=" ">
      <text variable="title" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      <text term="in" form="short"/>
      <text macro="editor" strip-periods="true" suffix=","/>
      <text macro="container-title" font-style="italic"/>
    </group>
    <text macro="series-info" prefix=", "/>
    <text macro="edition" prefix=", "/>
    <text macro="publisher-place-year"/>
    <text variable="page-first" prefix="&lt;page-first&gt; " suffix="&lt;/page-first&gt;"/>
  </macro>
  <macro name="render-article-journal">
    <group delimiter=" ">
      <text variable="title" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      <date form="text" variable="issued" date-parts="year" prefix="&lt;issued&gt;(" suffix=")&lt;/issued&gt;"/>
      <group delimiter=":">
        <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
        <text variable="issue" prefix="&lt;issue&gt;" suffix="&lt;/issue&gt;"/>
      </group>
      <text macro="container-title"/>
      <text variable="page-first" prefix="&lt;page-first&gt;" suffix="&lt;/page-first&gt;"/>
    </group>
    <text macro="internet-location"/>
  </macro>
  <macro name="render-book">
    <group delimiter=", ">
      <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      <text macro="edition"/>
      <text macro="translator"/>
      <text macro="editor"/>
      <text macro="series-info"/>
    </group>
    <text macro="publisher-place-year"/>
  </macro>
  <macro name="render-report">
    <group delimiter=", ">
      <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      <group delimiter=" ">
        <text variable="collection-title" strip-periods="true" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
        <text macro="genre"/>
        <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
      </group>
    </group>
    <text macro="publisher-place-year"/>
  </macro>
  <macro name="render-thesis">
    <group delimiter=" ">
      <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      <text macro="genre" prefix="(" suffix=","/>
      <text variable="publisher" suffix=",&lt;/publisher&gt;" prefix="&lt;publisher&gt;"/>
      <date form="text" variable="issued" date-parts="year" suffix=") [unpublished]&lt;/issued&gt;" prefix="&lt;issued&gt;"/>
    </group>
  </macro>
  <macro name="render-article-newspaper">
    <group delimiter=" ">
      <text variable="title" quotes="true" suffix=",&lt;/title&gt;" prefix="&lt;title&gt;"/>
      <text macro="container-title" font-style="italic"/>
      <text macro="issued-long" prefix="(" suffix=")"/>
      <text variable="page-first" prefix="&lt;page-first&gt;" suffix="&lt;/page-first&gt;"/>
    </group>
    <text macro="internet-location"/>
  </macro>
  <macro name="render-article-magazine">
    <group delimiter=" ">
      <text variable="title" quotes="true" suffix=",&lt;/title&gt;" prefix="&lt;title&gt;"/>
      <text macro="container-title" font-style="italic"/>
      <text macro="issued-long" prefix="(" suffix=")"/>
      <text variable="page-first" prefix="&lt;page-first&gt;" suffix="&lt;/page-first&gt;"/>
    </group>
    <text macro="internet-location"/>
  </macro>
  <macro name="render-webpage">
    <group delimiter=" ">
      <text variable="title" quotes="true" suffix=",&lt;/title&gt;" prefix="&lt;title&gt;"/>
      <text macro="issued-long" prefix="(" suffix=")"/>
    </group>
    <group delimiter=" ">
      <text term="online" prefix=", " suffix=":"/>
      <text macro="container-title" font-style="italic"/>
      <text variable="URL" prefix="&lt;URL&gt;&lt;" suffix="&gt;&lt;/URL&gt;"/>
    </group>
  </macro>
  
  <macro name="render-bill">
    <group delimiter=" ">
      <group delimiter=", ">
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        <text macro="author-note" strip-periods="true"/>
        <text macro="issued-long"/>
        <text macro="container-title"/>
      </group>
      <text macro="references" prefix="(" suffix=")"/>
      <text variable="title" prefix="&lt;title&gt;[" suffix="]&lt;/title&gt;" font-style="italic" form="short"/>
    </group>
  </macro>
  <macro name="render-case">
    <choose>
      <if variable="author">
        <group delimiter=", ">
          <text variable="authority" strip-periods="true" prefix="&lt;authority&gt;" suffix="&lt;/authority&gt;"/>
          <date form="numeric" variable="issued" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
          <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
          <group delimiter=" ">
            <text variable="title" font-style="italic" strip-periods="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
            <text variable="references" prefix="&lt;references&gt;(" suffix=")&lt;/references&gt;"/>
          </group>
          <text variable="container-title" strip-periods="true" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
          <text variable="note" prefix="&lt;note&gt;" suffix="&lt;/note&gt;"/>
        </group>
      </if>
      <else-if variable="title" match="none">
        <group delimiter=", ">
          <text variable="authority" strip-periods="true" prefix="&lt;authority&gt;" suffix="&lt;/authority&gt;"/>
          <date form="numeric" variable="issued" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
          <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
          <group delimiter=" ">
            <text variable="title" form="short" font-style="italic" strip-periods="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
            <text variable="references" prefix="&lt;references&gt;(" suffix=")&lt;/references&gt;"/>
          </group>
          <text variable="container-title" strip-periods="true" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
          <text variable="note" prefix="&lt;note&gt;" suffix="&lt;/note&gt;"/>
        </group>
      </else-if>
      <else>
        <text variable="title" font-style="italic" suffix=", &lt;/title&gt;" strip-periods="true" prefix="&lt;title&gt;"/>
        <choose>
          <if variable="container-title" match="none">
            <group delimiter=" ">
              <date form="text" variable="issued" date-parts="year" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
              <text variable="authority" strip-periods="true" prefix="&lt;authority&gt;" suffix="&lt;/authority&gt;"/>
              <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
            </group>
          </if>
          <else>
            <date form="text" variable="issued" date-parts="year" prefix="&lt;issued&gt;[" suffix="] &lt;/issued&gt;"/>
            <text variable="volume" suffix=" &lt;/volume&gt;" prefix="&lt;volume&gt;"/>
            <text variable="container-title" suffix=" &lt;/container-title&gt;" strip-periods="true" prefix="&lt;container-title&gt;"/>
            <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
            <text variable="authority" prefix="&lt;authority&gt; (" suffix=")&lt;/authority&gt;" strip-periods="true"/>
          </else>
        </choose>
        <text variable="note" prefix="&lt;note&gt;, " suffix="&lt;/note&gt;"/>
        <text variable="references" prefix="&lt;references&gt; (" suffix=")&lt;/references&gt;"/>
      </else>
    </choose>
    <text variable="URL" prefix="&lt;URL&gt; (available on " suffix=")&lt;/URL&gt;"/>
  </macro>
  <macro name="pinpoint">
    <group delimiter=" ">
      <choose>
        <if locator="page">
          <choose>
            <if variable="locator">
              <text term="at" prefix=" "/>
            </if>
          </choose>
        </if>
        <else>
          <label variable="locator" plural="contextual" form="short" strip-periods="true" prefix=", "/>
        </else>
      </choose>
      <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
    </group>
  </macro>
  <macro name="short-form">
    
    <choose>
      <if type="bill legal_case legislation" match="none">
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name and="symbol" form="short" delimiter-precedes-last="never"/>
          <substitute>
            <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
              <name and="symbol" form="short"/>
            </names>
          </substitute>
        </names>
      </if>
      <else>
        <choose>
          <if type="legal_case">
            <choose>
              <if variable="author">
                <text variable="authority" suffix=", &lt;/authority&gt;" prefix="&lt;authority&gt;"/>
                <date form="numeric" variable="issued" suffix=", &lt;/issued&gt;" prefix="&lt;issued&gt;"/>
              </if>
              <else-if variable="title" match="none">
                <text variable="authority" suffix=", &lt;/authority&gt;" prefix="&lt;authority&gt;"/>
                <date form="numeric" variable="issued" suffix=", &lt;/issued&gt;" prefix="&lt;issued&gt;"/>
              </else-if>
            </choose>
          </if>
        </choose>
        <text variable="title" font-style="italic" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher-place-year">
    <group delimiter=", " prefix=" (" suffix=")">
      <group delimiter=": ">
        <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </group>
      <date form="text" variable="issued" date-parts="year" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
    </group>
  </macro>
  <citation et-al-min="4" et-al-use-first="1">
    
    <layout delimiter="; " suffix=".">
      <choose>
        
        <if position="ibid-with-locator">
          <group>
            <text term="ibid" font-style="italic" strip-periods="true"/>
            <text macro="pinpoint"/>
          </group>
        </if>
        <else-if position="ibid">
          <text term="ibid" font-style="italic" strip-periods="true"/>
        </else-if>
        
        <else-if position="subsequent">
          <group delimiter=" ">
            <group delimiter=", ">
              <text macro="short-form"/>
              <text value="supra" font-style="italic"/>
            </group>
            <text value="note"/>
            <text variable="first-reference-note-number" prefix="&lt;first-reference-note-number&gt;" suffix="&lt;/first-reference-note-number&gt;"/>
          </group>
          <text macro="pinpoint"/>
        </else-if>
        <else>
          <group>
            <group delimiter=", ">
              <choose>
                <if type="bill legislation" match="any">
                  <text macro="render-bill"/>
                </if>
                <else-if type="legal_case">
                  <text macro="render-case"/>
                </else-if>
                <else>
                  <text macro="author-note" strip-periods="true"/>
                  <choose>
                    <if type="article-journal">
                      <text macro="render-article-journal"/>
                    </if>
                    <else-if type="chapter">
                      <text macro="render-chapter"/>
                    </else-if>
                    <else-if type="thesis">
                      <text macro="render-thesis"/>
                    </else-if>
                    <else-if type="article-newspaper" match="any">
                      <text macro="render-article-newspaper"/>
                    </else-if>
                    <else-if type="article-magazine">
                      <text macro="render-article-magazine"/>
                    </else-if>
                    <else-if type="webpage post-weblog" match="any">
                      <text macro="render-webpage"/>
                    </else-if>
                    <else-if type="book">
                      <text macro="render-book"/>
                    </else-if>
                    <else-if type="report">
                      <text macro="render-report"/>
                    </else-if>
                    <else>
                      <group delimiter=" ">
                        <group delimiter=", ">
                          <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                          <text macro="translator"/>
                          <text macro="edition"/>
                        </group>
                        <text macro="publisher-place-year"/>
                      </group>
                    </else>
                  </choose>
                </else>
              </choose>
            </group>
            <text macro="pinpoint"/>
          </group>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography et-al-min="4" et-al-use-first="1" subsequent-author-substitute="———">
    <sort>
      <key macro="sort-by-type"/>
      <key macro="author-bib"/>
      <key variable="issued"/>
    </sort>
    <layout suffix=".">
      <choose>
        <if type="bill legal_case legislation" match="any">
          <choose>
            <if type="bill legislation" match="any">
              <text macro="render-bill"/>
            </if>
            <else-if type="legal_case">
              <text macro="render-case"/>
            </else-if>
          </choose>
        </if>
        <else>
          <group delimiter=". ">
            <text macro="author-bib" strip-periods="true"/>
            <choose>
              <if type="article-journal">
                <text macro="render-article-journal"/>
              </if>
              <else-if type="chapter">
                <text macro="render-chapter"/>
              </else-if>
              <else-if type="thesis">
                <text macro="render-thesis"/>
              </else-if>
              <else-if type="article-newspaper">
                <text macro="render-article-newspaper"/>
              </else-if>
              <else-if type="article-magazine">
                <text macro="render-article-magazine"/>
              </else-if>
              <else-if type="book">
                <text macro="render-book"/>
              </else-if>
              <else-if type="webpage post-weblog" match="any">
                <text macro="render-webpage"/>
              </else-if>
              <else-if type="report">
                <text macro="render-report"/>
              </else-if>
              <else>
                <group>
                  <group delimiter=", ">
                    <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                    <text macro="edition"/>
                    <text macro="translator"/>
                    <text macro="editor"/>
                  </group>
                  <text macro="publisher-place-year"/>
                </group>
              </else>
            </choose>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>