      <key macro="author"/>
      <key macro="year-date"/>
      <key macro="title"/>
    </sort>
    <layout suffix=".">
      <choose>
        <if type="book interview manuscript map patent legislation legal_case report thesis" match="any">
          <group suffix=". ">
            <text macro="author"/>
            <text macro="year-date"/>
            <text macro="title"/>
          </group>
          <group delimiter=", ">
            <text macro="container-author"/>
            <text macro="translator"/>
            <text macro="editor"/>
            <text macro="collection-editor"/>
            <text variable="section" prefix="&lt;section&gt;" suffix="&lt;/section&gt;"/>
            <text macro="collection"/>
            <text macro="scale"/>
            <text macro="edition"/>
            <text macro="version"/>
            <text macro="medium"/>
            <text macro="genre"/>
            <text macro="volume"/>
            <text variable="container-title" prefix="&lt;container-title&gt;cod. " suffix="&lt;/container-title&gt;"/>
            <text macro="number-of-volumes"/>
            <text macro="number"/>
            <text macro="call-number"/>
            <text macro="publisher-place"/>
            <text macro="number-of-pages"/>
            <text macro="page"/>
            <text macro="isbn"/>
            <text macro="note"/>
            <text macro="url"/>
            <text macro="accessed"/>
          </group>
        </if>
        <else-if type="article-journal article-magazine article-newspaper post-weblog" match="any">
          <group suffix=". ">
            <text macro="author"/>
            <text macro="year-date"/>
            <text macro="title"/>
          </group>
          <group delimiter=", ">
            <text macro="container-title"/>
            <text macro="date"/>
            <text macro="section" prefix="sect. "/>
            <text macro="edition"/>
            <text macro="volume"/>
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            <text macro="issn"/>
            <text macro="doi"/>
            <text macro="note"/>
            <text macro="url"/>
            <text macro="accessed"/>
          </group>
        </else-if>
        <else-if type="paper-conference chapter entry-encyclopedia entry-dictionary entry" match="any">
          <group suffix=". ">
            <text macro="author"/>
            <text macro="year-date"/>
            <text macro="title"/>
          </group>
          <group delimiter=", " prefix="In: ">
            <text macro="container-author"/>
            <text macro="translator"/>
            <text macro="editor"/>
            <text macro="collection-editor"/>
            <text macro="container-title"/>
            <text macro="event"/>
            <text macro="date"/>
            <text macro="collection"/>
            <text macro="edition"/>
            <text macro="volume"/>
            <text macro="number-of-volumes"/>
            <text macro="publisher-place"/>
            <text macro="page"/>
            <text macro="isbn"/>
            <text macro="doi"/>
            <text macro="note"/>
            <text macro="url"/>
            <text macro="accessed"/>
          </group>
        </else-if>
        <else>
          <group suffix=". ">
            <text macro="responsability"/>
            <text macro="year-date"/>
            <text macro="title"/>
          </group>
          <group delimiter=", ">
            <text variable="section" prefix="&lt;section&gt;" suffix="&lt;/section&gt;"/>
            <text macro="event"/>
            <text macro="collection"/>
            <text macro="scale"/>
            <text macro="edition"/>
            <text macro="version"/>
            <text macro="medium"/>
            <text macro="genre"/>
            <text macro="volume"/>
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <text macro="date"/>
            <text macro="number-of-volumes"/>
            <text macro="number"/>
            <text macro="authority"/>
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            <text macro="publisher"/>
            <text macro="call-number"/>
            <text macro="number-of-pages"/>
            <text macro="page"/>
            <text macro="isbn"/>
            <text macro="note"/>
            <text macro="url"/>
            <text macro="accessed"/>
          </group>
        </else>
      </choose>
