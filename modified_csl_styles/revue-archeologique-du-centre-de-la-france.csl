<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="fr-FR">
  <info>
    <title>Revue archéologique du Centre de la France (French)</title>
    <title-short>RACF</title-short>
    <id>http://www.zotero.org/styles/revue-archeologique-du-centre-de-la-france</id>
    <link href="http://www.zotero.org/styles/revue-archeologique-du-centre-de-la-france" rel="self"/>
    <link href="http://racf.revues.org/920#tocto2n12" rel="documentation"/>
    <author>
      <name>Francois Capron</name>
      <email>f.capron37@gmail.com</email>
    </author>
    <contributor>
      <name>Benedicte MacGregor</name>
    </contributor>
    <contributor>
      <name>Matthieu Gaultier</name>
    </contributor>
    <category citation-format="author-date"/>
    <category field="history"/>
    <category field="anthropology"/>
    <issn>0220-6617</issn>
    <eissn>1951-6207</eissn>
    <updated>2016-09-28T13:09:49+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author" suffix=". - &lt;/author&gt;" prefix="&lt;author&gt;">
      <name sort-separator=" " initialize-with=". " name-as-sort-order="all" delimiter=", " delimiter-precedes-last="never" and="text"/>
      <label form="short" prefix=", " suffix=". " text-case="uppercase" strip-periods="true"/>
      <substitute>
        <text macro="editor-biblio" suffix=" (dir.) - "/>
      </substitute>
    </names>
  </macro>
  <macro name="editor-biblio">
    <names variable="editor" suffix=". &lt;/editor&gt;" prefix="&lt;editor&gt;">
      <name sort-separator=" " initialize-with=". " name-as-sort-order="all" delimiter=", " delimiter-precedes-last="never" and="text"/>
    </names>
  </macro>
  <macro name="journal-no-author">
    <group suffix=",">
      <text variable="container-title" form="short" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      <text variable="volume" prefix="&lt;volume&gt;, " form="short" suffix="&lt;/volume&gt;"/>
      <text variable="issue" prefix="&lt;issue&gt;, " form="short" suffix="&lt;/issue&gt;"/>
    </group>
  </macro>
  <macro name="author-short">
    <choose>
      <if variable="author">
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name form="short" and="text" delimiter=", " initialize-with=". " delimiter-precedes-last="never" name-as-sort-order="all" sort-separator=" " et-al-min="4" et-al-use-first="1" font-variant="small-caps"/>
          <label form="short" prefix=", " suffix=". " text-case="uppercase" strip-periods="true"/>
          <et-al font-style="italic" font-variant="normal"/>
        </names>
      </if>
      <else-if variable="editor">
        <text macro="editor"/>
      </else-if>
      <else-if type="book">
        <text variable="title-short" suffix=" ...&lt;/title-short&gt;" prefix="&lt;title-short&gt;"/>
      </else-if>
      <else-if type="article-journal">
        <text macro="journal-no-author"/>
      </else-if>
    </choose>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text value="[s.d.]"/>
      </else>
    </choose>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name form="short" and="text" delimiter=", " initialize-with=". " delimiter-precedes-last="never" name-as-sort-order="all" sort-separator=" " et-al-min="4" et-al-use-first="1">
        <name-part name="family" font-variant="small-caps"/>
        <name-part name="given" font-variant="small-caps"/>
      </name>
    </names>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="manuscript" match="none">
        <choose>
          <if variable="publisher">
            <text variable="publisher" suffix=", &lt;/publisher&gt;" prefix="&lt;publisher&gt;"/>
          </if>
          <else>
            <text value="[s.n.], "/>
          </else>
        </choose>
      </if>
    </choose>
    <choose>
      <if type="manuscript">
        <text variable="archive" prefix="&lt;archive&gt;lieu de consultation : " suffix=", &lt;/archive&gt;"/>
        <text variable="call-number" suffix=", &lt;/call-number&gt;" prefix="&lt;call-number&gt;"/>
      </if>
      <else-if type="report">
        <text variable="archive" prefix="&lt;archive&gt;lieu de consultation : " suffix=", &lt;/archive&gt;"/>
      </else-if>
      <else>
        <choose>
          <if variable="publisher-place">
            <text variable="publisher-place" suffix=", &lt;/publisher-place&gt;" prefix="&lt;publisher-place&gt;"/>
          </if>
          <else>
            <text value="[s.l.] , "/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text value="Disponible sur : " prefix=" "/>
        <group prefix="&lt; " suffix=" &gt;">
          <text value="https://doi.org/"/>
          <text variable="DOI" prefix="&lt;DOI&gt;" suffix="&lt;/DOI&gt;"/>
        </group>
      </if>
      <else-if variable="URL">
        <text value="URL : " prefix=". "/>
        <group>
          <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="accessed-date">
    <choose>
      <if variable="URL">
        <group prefix=", " delimiter=" ">
          <text value="consulté le"/>
          <date variable="accessed">
            <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
            <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
            <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
          </date>
        </group>
      </if>
      <else-if variable="DOI">
        <group prefix="(" suffix=")" delimiter=" ">
          <text value="consulté le"/>
          <date variable="accessed">
            <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
            <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
            <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
          </date>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="journal-title">
    <choose>
      <if type="chapter paper-conference article-journal article-magazine article-newspaper webpage" match="any">
        <group suffix=", ">
          <text variable="container-title" font-style="italic" form="short" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          <choose>
            <if variable="URL DOI" match="any">
              <text value="en ligne" prefix=" [" suffix="]" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
      </if>
      <else>
        <text variable="container-title" font-style="italic" suffix=". &lt;/container-title&gt;" form="short" prefix="&lt;container-title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <group delimiter=" ">
      <choose>
        <if type="chapter paper-conference article-journal article-magazine article-newspaper webpage" match="none">
          <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          <choose>
            <if variable="URL DOI" match="any">
              <text value="en ligne" prefix=" [" suffix="]" text-case="capitalize-first"/>
            </if>
          </choose>
        </if>
        <else>
          <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short" suffix="." strip-periods="true"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix=".&lt;/edition&gt;" prefix="&lt;edition&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="collection">
    <choose>
      <if variable="collection-number">
        <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
        <text variable="collection-number" prefix="&lt;collection-number&gt; n° " suffix=", &lt;/collection-number&gt;"/>
      </if>
      <else>
        <text variable="collection-title" prefix="&lt;collection-title&gt;(" suffix="). &lt;/collection-title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if type="report" match="any">
        <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
      </if>
      <else>
        <text variable="number-of-pages" prefix="&lt;number-of-pages&gt;" suffix="&lt;/number-of-pages&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="locators">
    <text variable="volume" prefix="&lt;volume&gt; Vol. " suffix=",&lt;/volume&gt;"/>
    <text variable="issue" prefix="&lt;issue&gt; n?" suffix=",&lt;/issue&gt;"/>
    <text variable="page" prefix="&lt;page&gt; p. " suffix=".&lt;/page&gt;"/>
  </macro>
  <macro name="webpageinfo">
    <text variable="note" prefix="&lt;note&gt;Tome " suffix=" | &lt;/note&gt;"/>
    <text macro="year-date" suffix=","/>
  </macro>
  <macro name="parution">
    <date variable="issued">
      <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
      <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
      <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <citation disambiguate-add-year-suffix="true" disambiguate-add-names="true" disambiguate-add-givenname="true" givenname-disambiguation-rule="primary-name">
    <layout prefix="(" suffix=")" delimiter=" ; ">
      <group delimiter=" : ">
        <group delimiter=" ">
          <text macro="author-short"/>
          <text macro="year-date"/>
          <text variable="year-suffix" prefix="&lt;year-suffix&gt; " suffix="&lt;/year-suffix&gt;"/>
        </group>
        <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="1" line-spacing="1">
    <sort>
      <key macro="author-short"/>
      <key variable="issued"/>
    </sort>
    <layout>
      <group delimiter=" ">
        <text macro="author-short"/>
        <text macro="year-date"/>
        <text variable="year-suffix" prefix="&lt;year-suffix&gt; " suffix="&lt;/year-suffix&gt;"/>
      </group>
      <group display="block" delimiter="">
        <text macro="author"/>
        <text macro="title" suffix=", "/>
        <choose>
          <if type="thesis">
            <text variable="genre" suffix=". &lt;/genre&gt;" prefix="&lt;genre&gt;"/>
            <text macro="publisher"/>
            <text variable="number-of-pages" suffix=" p.&lt;/number-of-pages&gt;" prefix="&lt;number-of-pages&gt;"/>
          </if>
          <else-if type="bill book graphic legal_case legislation motion_picture report song manuscript" match="any">
            <text variable="genre" suffix=", &lt;/genre&gt;" font-style="italic" prefix="&lt;genre&gt;"/>
            <text macro="collection"/>
            <text macro="publisher"/>
            <text macro="edition"/>
            <text macro="pages" suffix=" p."/>
          </else-if>
          <else-if type="patent">
            <text variable="number" suffix=". &lt;/number&gt;" prefix="&lt;number&gt;"/>
            <date variable="issued">
              <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
              <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
              <date-part name="year" suffix=".&lt;/year&gt;" prefix="&lt;year&gt;"/>
            </date>
          </else-if>
          <else-if type="chapter paper-conference" match="any">
            <group prefix=" ">
              <text value="in" suffix=" : " font-style="italic"/>
              <text macro="editor-biblio" suffix=" (dir.)"/>
              <text variable="event" prefix="&lt;event&gt;, " font-style="italic" suffix="&lt;/event&gt;"/>
              <text macro="journal-title" prefix=", "/>
            </group>
            <text variable="collection-title" suffix=", &lt;/collection-title&gt;" prefix="&lt;collection-title&gt;"/>
            <text macro="publisher"/>
            <text variable="page" prefix="&lt;page&gt; p. " suffix=".&lt;/page&gt;"/>
          </else-if>
          <else-if type="webpage" match="any">
            <group prefix=" ">
              <text macro="editor"/>
              <text macro="journal-title"/>
              <text variable="event" suffix=". &lt;/event&gt;" font-style="italic" prefix="&lt;event&gt;"/>
            </group>
            <text macro="webpageinfo" prefix=" "/>
            <text variable="page" prefix="&lt;page&gt; p. " suffix=".&lt;/page&gt;"/>
            <text macro="parution" prefix=" mis en ligne le "/>
            <text macro="accessed-date"/>
            <text macro="access"/>
          </else-if>
          <else-if type="article-journal" match="any">
            <group prefix=" ">
              <text macro="journal-title"/>
            </group>
            <text variable="volume" form="short" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
            <text variable="issue" prefix="&lt;issue&gt;, " form="short" suffix="&lt;/issue&gt;"/>
            <text variable="page" prefix="&lt;page&gt; : " suffix=".&lt;/page&gt;"/>
          </else-if>
          <else>
            <text macro="journal-title"/>
            <group suffix=".">
              <date variable="issued" suffix=".">
                <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
                <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
                <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
              </date>
            </group>
            <group suffix=". ">
              <text macro="locators"/>
            </group>
          </else>
        </choose>
      </group>
    </layout>
  </bibliography>
</style>