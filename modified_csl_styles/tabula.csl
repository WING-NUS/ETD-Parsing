  <locale>
    <terms>
      <term name="anonymous">Anon.</term>
      <term name="no date">[sin fecha]</term>
      <term name="in">en</term>
      <term name="online">en línea</term>
      <term name="accessed">consulta:</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name initialize="false" and="text" name-as-sort-order="all" sort-separator=", " delimiter=", " delimiter-precedes-last="never">
        <name-part name="family" text-case="uppercase"/>
        <name-part name="given"/>
      </name>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name initialize="false" and="text" name-as-sort-order="all" sort-separator=", " delimiter=", " delimiter-precedes-last="never">
        <name-part name="family" text-case="uppercase"/>
        <name-part name="given"/>
      </name>
      <label prefix=" (" form="short" suffix=".)"/>
    </names>
  </macro>
  <macro name="translator">
    <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;">
      <name and="text" name-as-sort-order="all" sort-separator=", " delimiter=", " delimiter-precedes-last="never">
        <name-part name="family" text-case="uppercase"/>
        <name-part name="given"/>
      </name>
      <label prefix=" (" form="short" suffix=".)"/>
    </names>
  </macro>
  <macro name="author-citation">
    <choose>
      <if variable="author editor translator title title-short" match="any">
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name form="short" and="text" sort-separator=", " name-as-sort-order="all" delimiter=", " delimiter-precedes-last="never"/>
          <substitute>
            <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
            <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
            <text variable="title-short" prefix="&lt;title-short&gt;«" suffix="»&lt;/title-short&gt;"/>
            <text variable="title" prefix="&lt;title&gt;«" suffix="»&lt;/title&gt;"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="container-author">
    <names variable="container-author" prefix="&lt;container-author&gt;" suffix="&lt;/container-author&gt;">
      <name initialize="false" and="text" name-as-sort-order="all" sort-separator=", " delimiter=", " delimiter-precedes-last="never" et-al-min="4" et-al-use-first="1">
        <name-part name="family" text-case="uppercase"/>
        <name-part name="given"/>
      </name>
      <substitute>
        <text macro="editor"/>
        <text macro="translator"/>
        <text variable="note" prefix="&lt;note&gt;" suffix="&lt;/note&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" form="long" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="book thesis map motion_picture song manuscript" match="any">
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else-if type="paper-conference speech chapter article-journal article-magazine article-newspaper post-weblog post webpage broadcast entry-dictionary entry-encyclopedia report" match="any">
        <text variable="title" prefix="&lt;title&gt;&quot;" suffix="&quot;. &lt;/title&gt;"/>
        <choose>
          <if variable="container-author editor note" match="any">
            <text term="in" text-case="capitalize-first" suffix=": "/>
            <text macro="container-author"/>
            <choose>
              <if variable="container-title event" match="any">
                <text value=". "/>
              </if>
            </choose>
          </if>
        </choose>
        <choose>
          <if variable="container-title">
            <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </if>
          <else>
            <text variable="event" font-style="italic" prefix="&lt;event&gt;" suffix="&lt;/event&gt;"/>
          </else>
        </choose>
      </else-if>
      <else-if type="report">
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else-if type="patent">
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else>
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
    <choose>
      <if variable="URL">
        <text term="online" prefix=" [" suffix="]"/>
      </if>
    </choose>
  </macro>
  <macro name="number">
    <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
  </macro>
  <macro name="medium">
    <text variable="medium" prefix="&lt;medium&gt;" suffix="&lt;/medium&gt;"/>
  </macro>
  <macro name="genre">
    <choose>
      <if type="map">
        <choose>
          <if variable="genre">
            <text variable="genre" prefix="&lt;genre&gt;[" suffix="]&lt;/genre&gt;"/>
          </if>
          <else>
            <text value="carte" prefix="[" suffix="]"/>
          </else>
        </choose>
      </if>
      <else>
        <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
          <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <text variable="edition" form="long" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
  </macro>
  <macro name="publisher-place">
    <choose>
      <if type="patent manuscript article-newspaper broadcast motion_picture song entry-encyclopedia entry-dictionary" match="any">
        <choose>
          <if variable="publisher-place">
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
          </if>
        </choose>
      </if>
      <else>
        <choose>
          <if variable="publisher-place">
            <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
          </if>
          <else>
            <text value="s.l." text-case="capitalize-first"/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="issue">
    <group delimiter=", ">
      <text variable="volume" prefix="&lt;volume&gt;vol. " suffix="&lt;/volume&gt;"/>
      <text variable="issue" prefix="&lt;issue&gt;n. " suffix="&lt;/issue&gt;"/>
      <text variable="page" prefix="&lt;page&gt;pp. " suffix="&lt;/page&gt;"/>
    </group>
  </macro>
  <macro name="publisher">
    <choose>
      <if type="broadcast motion_picture song report entry-encyclopedia entry-dictionary" match="any">
        <choose>
          <if variable="publisher">
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          </if>
        </choose>
      </if>
      <else>
        <choose>
          <if variable="publisher">
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          </if>
          <else>
            <text value="s.n."/>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="accessed">
    <choose>
      <if variable="URL">
        <group prefix=" [" suffix="]">
          <text term="accessed" text-case="capitalize-first" suffix=" "/>
          <date variable="accessed" form="numeric">
            <date-part name="day" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
            <date-part name="month" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
            <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
          </date>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="collection">
    <group delimiter=", ">
      <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
      <choose>
        <if variable="collection-number">
          <text variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
        </if>
        <else>
          <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="page">
    <choose>
      <if type="book thesis manuscript" match="any">
        <text variable="number-of-pages" suffix=" p&lt;/number-of-pages&gt;" prefix="&lt;number-of-pages&gt;"/>
      </if>
      <else-if type="chapter paper-conference article-newspaper" match="any">
        <text variable="page" prefix="&lt;page&gt;pp. " suffix="&lt;/page&gt;"/>
      </else-if>
      <else-if type="report patent" match="any">
        <text variable="page" suffix=" p&lt;/page&gt;" prefix="&lt;page&gt;"/>
      </else-if>
    </choose>
  </macro>
  <macro name="isbn">
    <text variable="ISBN" prefix="&lt;ISBN&gt;ISBN " suffix="&lt;/ISBN&gt;"/>
  </macro>
  <macro name="issn">
    <text variable="ISSN" prefix="&lt;ISSN&gt;ISSN " suffix="&lt;/ISSN&gt;"/>
  </macro>
  <macro name="doi">
    <text variable="DOI" prefix="&lt;DOI&gt;DOI " suffix="&lt;/DOI&gt;"/>
  </macro>
  <macro name="url">
    <choose>
      <if variable="URL">
        <group>
          <text variable="URL" prefix="&lt;URL&gt;&lt;" suffix="&gt;&lt;/URL&gt;"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="archive">
    <text variable="archive" prefix="&lt;archive&gt;" suffix="&lt;/archive&gt;"/>
    <choose>
      <if variable="archive_location">
        <text value=": "/>
      </if>
    </choose>
  </macro>
  <macro name="archive_location">
    <choose>
      <if variable="archive_location">
        <text variable="archive_location" prefix="&lt;archive_location&gt;" suffix="&lt;/archive_location&gt;"/>
      </if>
      <else>
        <text variable="call-number" prefix="&lt;call-number&gt;" suffix="&lt;/call-number&gt;"/>
      </else>
    </choose>
  </macro>
  <citation et-al-min="4" et-al-use-first="1" disambiguate-add-year-suffix="true" disambiguate-add-names="true" disambiguate-add-givenname="true" collapse="year" year-suffix-delimiter=", " after-collapse-delimiter=" ; ">
    <layout prefix="(" suffix=")" delimiter="; ">
      <group delimiter=", p. ">
        <group delimiter=", ">
          <text macro="author-citation"/>
          <text macro="year-date"/>
        </group>
        <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="author"/>
      <key macro="year-date"/>
      <key macro="title"/>
    </sort>
    <layout>
      <choose>
        <if type="book map" match="any">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="genre" suffix=". "/>
            <text macro="edition" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=". "/>
            <text macro="collection" suffix=". "/>
            <text macro="isbn" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </if>
        <else-if type="article-journal article-magazine" match="any">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=", "/>
            <text macro="edition" suffix=". "/>
            <text macro="issue" suffix=". "/>
            <text macro="issn" suffix=". "/>
            <text macro="doi" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="article-newspaper">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="edition" suffix=". "/>
            <text macro="publisher-place" suffix=", "/>
            <text macro="date" suffix=". "/>
            <text macro="page" suffix=". "/>
            <text macro="issn" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="entry entry-dictionary entry-encyclopedia" match="any">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="edition" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=". "/>
            <text macro="collection" suffix=", "/>
            <text macro="page" suffix=". "/>
            <text macro="isbn" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="chapter">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="edition" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=", "/>
            <text macro="collection" suffix=", "/>
            <text macro="page" suffix=". "/>
            <text macro="isbn" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="speech">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="genre" suffix=". "/>
            <text macro="publisher-place" suffix=". "/>
            <text macro="page" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="paper-conference">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="genre" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=", "/>
            <text macro="page" suffix=". "/>
            <text macro="isbn" suffix=". "/>
            <text macro="doi" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="thesis">
          <group>
            <text macro="author" suffix=", "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="genre" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="post-weblog post webpage" match="any">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="broadcast motion_picture song" match="any">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="medium" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=". "/>
            <text macro="collection" suffix=". "/>
            <text macro="isbn" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="report">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="genre" suffix=". "/>
            <text macro="edition" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=". "/>
            <text macro="collection" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="manuscript">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="genre" suffix=". "/>
            <text macro="edition" suffix=". "/>
            <text macro="publisher-place" suffix=". "/>
            <text macro="collection" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else-if type="patent">
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="number" suffix=". "/>
            <text macro="publisher-place" suffix=". "/>
            <text macro="collection" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else-if>
        <else>
          <group>
            <text macro="author" suffix=". "/>
            <text macro="year-date" prefix="(" suffix="). "/>
            <text macro="title" suffix=". "/>
            <text macro="medium" suffix=". "/>
            <text macro="genre" suffix=". "/>
            <text macro="date" suffix=". "/>
            <text macro="edition" suffix=". "/>
            <text macro="publisher-place" suffix=": "/>
            <text macro="publisher" suffix=". "/>
            <text macro="collection" suffix=". "/>
            <text macro="page" suffix=". "/>
            <text macro="isbn" suffix=". "/>
            <text macro="url" suffix=". "/>
            <text macro="accessed" suffix="."/>
          </group>
        </else>
      </choose>
      <group>
        <group display="block">
          <text macro="archive"/>
          <text macro="archive_location"/>
        </group>
      </group>
    </layout>
  </bibliography>
</style>