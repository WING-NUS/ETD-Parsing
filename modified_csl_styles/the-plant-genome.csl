  <macro name="editor">
    <names variable="editor" delimiter=", " prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name and="text" initialize-with="." delimiter=", "/>
      <label form="short" plural="never" prefix=", "/>
    </names>
  </macro>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name name-as-sort-order="first" and="text" sort-separator=", " initialize-with="." delimiter=", " delimiter-precedes-last="always"/>
      <label form="short" prefix=" "/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="text" delimiter=", " initialize-with="."/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if type="book webpage" match="any">
        <text variable="title" text-case="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", " suffix=".">
      <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
    </group>
  </macro>
  <macro name="year-date">
    <date variable="issued">
      <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix=".&lt;/edition&gt;" prefix="&lt;edition&gt;"/>
      </else>
    </choose>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" collapse="year-suffix">
    <sort>
      <key macro="year-date"/>
      <key variable="author"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <group delimiter=", ">
        <text macro="author-short"/>
        <text macro="year-date"/>
        <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
      </group>
    </layout>
  </citation>
  <bibliography line-spacing="2" hanging-indent="true">
    <sort>
      <key macro="author" names-use-last="true"/>
      <key macro="year-date" names-use-last="true"/>
    </sort>
    <layout>
      <group delimiter=". ">
        <text macro="author"/>
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
        <choose>
          <if type="book report chapter" match="any">
            <group delimiter=". " prefix=" ">
              <text macro="title"/>
              <text macro="edition"/>
              <text macro="editor"/>
              <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
              <text variable="collection-number" prefix="&lt;collection-number&gt;No. " suffix="&lt;/collection-number&gt;"/>
              <text macro="publisher"/>
            </group>
          </if>
          <else-if type="paper-conference" match="any">
            <group>
              <text variable="title" text-case="capitalize-first" prefix="&lt;title&gt;. " suffix="&lt;/title&gt;"/>
              <label text-case="capitalize-first" prefix=". " variable="page"/>
              <text variable="page" prefix="&lt;page&gt; " suffix="&lt;/page&gt;"/>
              <text variable="container-title" prefix="&lt;container-title&gt; in " suffix="&lt;/container-title&gt;"/>
              <text variable="event" prefix="&lt;event&gt; " suffix="&lt;/event&gt;"/>
              <text variable="publisher" prefix="&lt;publisher&gt;. " suffix="&lt;/publisher&gt;"/>
              <text variable="publisher-place" prefix="&lt;publisher-place&gt;, " suffix=".&lt;/publisher-place&gt;"/>
            </group>
          </else-if>
          <else-if type="webpage" match="any">
            <group delimiter=". " suffix=".">
              <text macro="title"/>
              <group delimiter=" ">
                <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
                <group delimiter=" " prefix="(" suffix=")">
                  <text term="accessed"/>
                  <date form="text" variable="accessed" prefix="&lt;accessed&gt; " suffix="&lt;/accessed&gt;"/>
                </group>
              </group>
            </group>
          </else-if>
          <else-if type="patent" match="any">
            <group delimiter=". ">
              <text macro="title" prefix=" "/>
              <text variable="note" prefix="&lt;note&gt; " suffix=", assignee&lt;/note&gt;"/>
              <group delimiter=" ">
                <text variable="note" prefix="&lt;note&gt;" suffix="&lt;/note&gt;"/>
                <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
              </group>
            </group>
          </else-if>
          <else-if type="thesis" match="any">
            <group prefix=" ">
              <text macro="title" suffix="."/>
              <text variable="genre" prefix="&lt;genre&gt; " suffix=" Thesis.&lt;/genre&gt;"/>
              <text variable="publisher" form="short" prefix="&lt;publisher&gt; " suffix=",&lt;/publisher&gt;"/>
              <text variable="publisher-place" prefix="&lt;publisher-place&gt; " suffix="&lt;/publisher-place&gt;"/>
            </group>
          </else-if>
          <else-if type="legislation" match="any">
            <group>
              <text macro="title" prefix=" "/>
              <text variable="abstract" prefix="&lt;abstract&gt;" suffix="&lt;/abstract&gt;"/>
              <text variable="volume" prefix="&lt;volume&gt;. " suffix="&lt;/volume&gt;"/>
              <text variable="container-title" prefix="&lt;container-title&gt;. " suffix="&lt;/container-title&gt;"/>
              <text variable="section" prefix="&lt;section&gt;. " suffix=":&lt;/section&gt;"/>
              <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
            </group>
          </else-if>
          <else>
            <group prefix=" ">
              <text macro="title"/>
              <text variable="container-title" form="short" font-style="normal" prefix="&lt;container-title&gt;. " suffix="&lt;/container-title&gt;"/>
              <group delimiter=":" prefix=" ">
                <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
              </group>
              <text variable="DOI" prefix="&lt;DOI&gt;. doi:" suffix="&lt;/DOI&gt;"/>
            </group>
          </else>
        </choose>
      </group>
    </layout>
  </bibliography>
