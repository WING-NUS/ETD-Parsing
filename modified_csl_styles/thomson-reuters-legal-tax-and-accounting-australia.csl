<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-GB">
  <info>
    <title>Thomson Reuters - Legal, Tax &amp; Accounting Australia</title>
    <id>http://www.zotero.org/styles/thomson-reuters-legal-tax-and-accounting-australia</id>
    <link href="http://www.zotero.org/styles/thomson-reuters-legal-tax-and-accounting-australia" rel="self"/>
    <link href="http://www.zotero.org/styles/australian-guide-to-legal-citation" rel="template"/>
    <link href="https://forums.zotero.org/discussion/4841/new-australian-legal-citation-style/?Focus=20831#Comment_20831" rel="documentation"/>
    <author>
      <name>Sebastian Karcher</name>
    </author>
    <category citation-format="note"/>
    <category field="law"/>
    <summary>Style for a series of Thomson Reuters law publications for Australia</summary>
    <updated>2012-10-25T21:15:26+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale>
    <terms>
      <term name="et-al">et al</term>
      <term name="open-quote">“</term>
      <term name="close-quote">”</term>
      <term name="open-inner-quote">‘</term>
      <term name="close-inner-quote">’</term>
      <term name="page-range-delimiter">–</term>
    </terms>
  </locale>
  
  <macro name="author-note">
    
    <choose>
      <if type="interview">
        <group delimiter=", ">
          <names variable="interviewer" prefix="&lt;interviewer&gt;" suffix="&lt;/interviewer&gt;">
            <name delimiter-precedes-last="never" and="text" delimiter=", " initialize="false" initialize-with=""/>
          </names>
          <names variable="author" prefix="&lt;author&gt;Interview with " suffix="&lt;/author&gt;">
            <name delimiter-precedes-last="never" and="text" delimiter=", " initialize="false" initialize-with=""/>
          </names>
        </group>
      </if>
      <else-if type="personal_communication">
        <group delimiter=" ">
          <group delimiter=" from ">
            <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
            <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
              <name delimiter-precedes-last="never" and="text" delimiter=", " initialize="false" initialize-with=""/>
            </names>
          </group>
          <names variable="recipient" prefix="&lt;recipient&gt;to " suffix="&lt;/recipient&gt;">
            <name delimiter-precedes-last="never" and="text" delimiter=", " initialize="false" initialize-with=""/>
          </names>
        </group>
      </else-if>
      <else-if type="broadcast">
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </else-if>
      <else-if type="legal_case legislation" match="any"/>
      <else>
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name delimiter-precedes-last="never" and="text" delimiter=", " initialize-with="" sort-separator=" " name-as-sort-order="all"/>
          <label form="short" prefix=" (" suffix=")" strip-periods="true"/>
          <substitute>
            <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
            <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
            <text macro="title"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="author-short">
    <choose>
      <if type="interview">
        <group delimiter=", ">
          <names variable="interviewer" prefix="&lt;interviewer&gt;" suffix="&lt;/interviewer&gt;">
            <name delimiter-precedes-last="never" and="text" form="short" delimiter=", " initialize="false" initialize-with=""/>
          </names>
          <names variable="author" prefix="&lt;author&gt;Interview with " suffix="&lt;/author&gt;">
            <name delimiter-precedes-last="never" and="text" form="short" delimiter=", " initialize="false" initialize-with=""/>
          </names>
        </group>
      </if>
      <else-if type="personal_communication">
        <group delimiter=" ">
          <group delimiter=" from ">
            <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
            <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
              <name delimiter-precedes-last="never" and="text" delimiter=", " form="short" initialize="false" initialize-with=""/>
            </names>
          </group>
          <names variable="recipient" prefix="&lt;recipient&gt;to " suffix="&lt;/recipient&gt;">
            <name delimiter-precedes-last="never" and="text" delimiter=", " initialize="false" form="short" initialize-with=""/>
          </names>
        </group>
      </else-if>
      <else-if type="broadcast">
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </else-if>
      <else>
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name delimiter-precedes-last="never" and="text" delimiter=", " initialize="false" initialize-with="" form="short"/>
          <substitute>
            <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
            <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
            <text macro="title"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="editor">
    <group>
      <choose>
        <if type="chapter paper-conference" match="any">
          <text term="in" suffix=" "/>
        </if>
      </choose>
      <names variable="editor translator" delimiter=", " prefix="&lt;editor translator&gt;" suffix="&lt;/editor translator&gt;">
        <name delimiter-precedes-last="never" and="text" delimiter=", " initialize-with="" name-as-sort-order="all" sort-separator=" "/>
        <label form="short" prefix=" (" suffix=")" strip-periods="true"/>
      </names>
    </group>
  </macro>
  
  <macro name="title">
    <choose>
      <if type="book legislation webpage thesis motion_picture manuscript" match="any">
        <text variable="title" font-style="italic" text-case="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else-if type="bill">
        <text variable="title" text-case="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else-if type="legal_case">
        <text variable="title" font-style="italic" strip-periods="true" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else>
        <text variable="title" quotes="true" text-case="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="title-short">
    <choose>
      <if type="book legislation webpage thesis motion_picture manuscript" match="any">
        <text variable="title" font-style="italic" text-case="title" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else>
        <text variable="title" quotes="true" text-case="title" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  
  <macro name="issued-year">
    <date variable="issued" form="text" date-parts="year" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
  </macro>
  <macro name="issued-full">
    <date variable="issued" form="text" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
  </macro>
  <macro name="date-news">
    <choose>
      <if type="article-newspaper broadcast personal_communication manuscript" match="any">
        <date variable="issued" form="text" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="date-parenthesis">
    <choose>
      <if type="legal_case" match="any">
        <choose>
          <if variable="volume">
            <text macro="issued-year" prefix="(" suffix=")"/>
          </if>
          <else-if variable="container-title volume number" match="any">
            
            <text macro="issued-year" prefix="[" suffix="]"/>
          </else-if>
        </choose>
      </if>
      <else-if type="article-journal article-magazine" match="any">
        <text macro="issued-year" prefix="(" suffix=")"/>
      </else-if>
      <else-if type="webpage">
        <text macro="issued-full" prefix="(" suffix=")"/>
      </else-if>
      <else-if type="legislation">
        <text macro="issued-year" font-style="italic"/>
      </else-if>
      <else-if type="bill">
        <text macro="issued-year"/>
      </else-if>
    </choose>
  </macro>
  
  <macro name="publisher">
    <choose>
      <if type="book chapter paper-conference article-newspaper report legislation motion_picture speech interview thesis" match="any">
        <group prefix="(" suffix=")" delimiter=", ">
          <choose>
            <if type="report thesis speech interview" match="any">
              <group delimiter=" ">
                <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
                <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
                <text variable="event" prefix="&lt;event&gt;at the " suffix="&lt;/event&gt;"/>
              </group>
            </if>
          </choose>
          <choose>
            <if type="article-newspaper">
              <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
            </if>
            <else-if type="legislation bill" match="any">
              
              <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            </else-if>
            <else>
              
              <names variable="director" prefix="&lt;director&gt;" suffix="&lt;/director&gt;">
                <label form="verb" text-case="capitalize-first" suffix=" "/>
                <name delimiter-precedes-last="never" and="text" delimiter=", " initialize="false" initialize-with=""/>
              </names>
              <choose>
                
                <if variable="publisher issued genre container-title" match="any">
                  <text macro="edition"/>
                </if>
              </choose>
              <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
              <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
              <choose>
                <if type="speech">
                  <text variable="event-place" prefix="&lt;event-place&gt;" suffix="&lt;/event-place&gt;"/>
                  <text macro="issued-full"/>
                </if>
                <else-if type="report interview" match="any">
                  <text macro="issued-full"/>
                </else-if>
                <else>
                  <text macro="issued-year"/>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="looseleaf">
    <choose>
      <if type="book">
        <choose>
          <if variable="publisher issued" match="none">
            <group prefix="(at " suffix=")">
              <choose>
                <if variable="edition">
                  <text variable="edition" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
                </if>
                <else>
                  <date variable="accessed" form="text" prefix="&lt;accessed&gt;" suffix="&lt;/accessed&gt;"/>
                </else>
              </choose>
            </group>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="volume-book">
    <choose>
      <if type="book chapter report" match="any">
        <group delimiter=" ">
          <label variable="volume" form="short" strip-periods="true"/>
          <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <label variable="edition" form="short" strip-periods="true"/>
        </group>
      </if>
      <else>
        <text variable="edition" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="book-container">
    <choose>
      <if type="chapter paper-conference" match="any">
        <group>
          <text macro="editor"/>
          <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;, " suffix="&lt;/container-title&gt;"/>
        </group>
      </if>
      <else-if type="webpage">
        <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </else-if>
    </choose>
  </macro>
  <macro name="broadcast-container">
    <choose>
      <if type="broadcast" match="any">
        <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="article-case-info">
    <choose>
      <if type="article-journal article-magazine article-newspaper legal_case" match="any">
        <group delimiter=" ">
          <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
          <choose>
            <if type="legal_case">
              <choose>
                <if variable="container-title">
                  <text variable="container-title" form="short" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                </if>
                <else-if variable="authority number" match="all">
                  
                  <group delimiter=" ">
                    <text variable="authority" form="short" strip-periods="true" prefix="&lt;authority&gt;" suffix="&lt;/authority&gt;"/>
                    <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
                    <text macro="issued-full" prefix="(" suffix=")"/>
                  </group>
                </else-if>
                <else>
                  <group delimiter=", " prefix="(" suffix=")">
                    <text value="Unreported"/>
                    <text variable="authority" prefix="&lt;authority&gt;" suffix="&lt;/authority&gt;"/>
                    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
                      <name name-as-sort-order="all" delimiter-precedes-last="never" and="text" delimiter=", " sort-separator=" "/>
                    </names>
                    <text macro="issued-full"/>
                  </group>
                </else>
              </choose>
            </if>
            <else>
              <text variable="container-title" form="short" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="page-and-locator">
    <choose>
      <if type="book chapter" match="none">
        <group delimiter=" ">
          <text variable="page-first" prefix="&lt;page-first&gt;" suffix="&lt;/page-first&gt;"/>
          <text macro="locator"/>
        </group>
      </if>
      <else>
        <text macro="locator"/>
      </else>
    </choose>
  </macro>
  <macro name="locator">
    <choose>
      <if type="book chapter" match="any">
        <group delimiter=" ">
          <label variable="locator" form="short" strip-periods="true"/>
          <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
        </group>
      </if>
      <else>
        <text variable="locator" prefix="&lt;locator&gt;at " suffix="&lt;/locator&gt;"/>
      </else>
    </choose>
  </macro>
  
  <macro name="manuscript-catchall">
    <choose>
      <if type="manuscript">
        <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="URL">
    <choose>
      <if type="legal_case legislation bill" match="none">
        <group delimiter=" ">
          <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
          <date variable="accessed" form="text" prefix="&lt;accessed&gt;viewed " suffix="&lt;/accessed&gt;"/>
        </group>
      </if>
    </choose>
  </macro>
  <citation et-al-min="20" et-al-use-first="19" et-al-subsequent-min="3" et-al-subsequent-use-first="1">
    <layout suffix="." delimiter="; ">
      <choose>
        <if position="subsequent">
          <choose>
            <if type="legal_case bill legislation manuscript" match="any">
              
              <group delimiter=" ">
                <group delimiter=", ">
                  <text macro="author-note"/>
                  <text macro="title"/>
                </group>
                
                <group delimiter=" ">
                  <text macro="date-parenthesis"/>
                  <text macro="article-case-info"/>
                  <text macro="book-container"/>
                  <text macro="publisher"/>
                </group>
              </group>
              <text macro="manuscript-catchall" prefix=", "/>
              <text macro="date-news" prefix=", "/>
              <group delimiter=", " prefix=" ">
                <text macro="looseleaf"/>
                <text variable="page-first" prefix="&lt;page-first&gt;" suffix="&lt;/page-first&gt;"/>
                <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
              </group>
            </if>
            <else>
              <group delimiter=", ">
                <text macro="author-short"/>
                <choose>
                  <if disambiguate="true">
                    <text macro="title-short"/>
                  </if>
                </choose>
                <choose>
                  <if type="book chapter" match="any">
                    <text variable="first-reference-note-number" prefix="&lt;first-reference-note-number&gt;n " suffix="&lt;/first-reference-note-number&gt;"/>
                    <text macro="locator"/>
                  </if>
                  <else>
                    <group delimiter=" ">
                      <text variable="first-reference-note-number" prefix="&lt;first-reference-note-number&gt;n " suffix="&lt;/first-reference-note-number&gt;"/>
                      <text macro="locator"/>
                    </group>
                  </else>
                </choose>
              </group>
            </else>
          </choose>
        </if>
        <else>
          
          <group delimiter=" ">
            <group delimiter=", ">
              <group delimiter=" ">
                <group delimiter=", ">
                  <text macro="author-note"/>
                  <text macro="title"/>
                  <text macro="broadcast-container"/>
                </group>
                <group delimiter=" ">
                  <text macro="date-parenthesis"/>
                  <text macro="article-case-info"/>
                  <text macro="book-container"/>
                  <text macro="publisher"/>
                </group>
              </group>
              <text macro="manuscript-catchall"/>
              <text macro="date-news"/>
            </group>
            <group delimiter=", ">
              <group delimiter=" ">
                <text macro="volume-book"/>
                <text macro="looseleaf"/>
              </group>
              <text macro="page-and-locator"/>
            </group>
          </group>
          <text macro="URL" prefix=", "/>
        </else>
      </choose>
    </layout>
  </citation>
</style>