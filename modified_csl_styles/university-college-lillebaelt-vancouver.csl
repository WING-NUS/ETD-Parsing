<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" class="in-text" page-range-format="minimal" default-locale="en-US">
  <info>
    <title>University College Lillebælt - Vancouver (English)</title>
    <id>http://www.zotero.org/styles/university-college-lillebaelt-vancouver</id>
    <link href="http://www.zotero.org/styles/university-college-lillebaelt-vancouver" rel="self"/>
    <link href="http://esdhweb.ucl.dk/480836.Formm%C3%A6ssige%20krav%202014.pdf" rel="documentation"/>
    <author>
      <name>Roald Erik Frøsig</name>
      <email>roaldfrosig@gmail.com</email>
    </author>
    <category citation-format="numeric"/>
    <updated>2014-12-17T13:56:00+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="online">internet</term>
      <term name="accessed">cited</term>
      <term name="available at">available from</term>
      <term name="no date">date unknown</term>
      <term name="page" form="short">p.</term>
    </terms>
  </locale>
  
  <macro name="page">
    <choose>
      <if is-numeric="locator">
        <label variable="locator" form="short" suffix=" "/>
        <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
      </if>
      <else>
        <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
      </else>
    </choose>
  </macro>
  
  <macro name="author">
    <choose>
      <if type="legislation">
      </if>
      <else-if type="entry-encyclopedia" variable="author" match="all">
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name name-as-sort-order="all" sort-separator=" " initialize-with=""/>
        </names>
      </else-if>
      <else-if variable="author editor composer" match="any">
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name name-as-sort-order="all" sort-separator=" " initialize-with=""/>
          <label prefix=", "/>
          <substitute>
            <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
            <names variable="composer" prefix="&lt;composer&gt;" suffix="&lt;/composer&gt;"/>
          </substitute>
        </names>
      </else-if>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="entry-dictionary entry-encyclopedia" match="any">
        <choose>
          <if variable="author">
            <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </if>
        </choose>
      </if>
      <else-if type="post-weblog" match="none">
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
    </choose>
  </macro>
  <macro name="editor-container">
    <group delimiter=": ">
      <text term="in" text-case="capitalize-first"/>
      <group delimiter=". ">
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
          <name name-as-sort-order="all" sort-separator=" " initialize-with=""/>
          <label prefix=", "/>
        </names>
        <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </group>
    </group>
  </macro>
  <macro name="container-title">
    <choose>
      <if type="article-journal">
        <choose>
          <if variable="container-title-short">
            <text variable="container-title-short" strip-periods="true" prefix="&lt;container-title-short&gt;" suffix="&lt;/container-title-short&gt;"/>
          </if>
          <else>
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </else>
        </choose>
      </if>
      <else-if type="entry-encyclopedia" variable="author">
        <text macro="editor-container"/>
      </else-if>
      <else-if type="chapter">
        <text macro="editor-container"/>
      </else-if>
      <else>
        <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="material">
    <choose>
      <if type="report">
        <text macro="online" prefix=" [" suffix="]"/>
      </if>
      <else-if type="thesis" variable="genre">
        <group delimiter=", ">
          <text variable="genre" prefix="&lt;genre&gt;[" suffix="]&lt;/genre&gt;"/>
          <text macro="online" prefix="[" suffix="]"/>
        </group>
      </else-if>
      <else-if variable="medium">
        <group delimiter=", ">
          <text variable="medium" prefix="&lt;medium&gt;[" suffix="]&lt;/medium&gt;"/>
          <text macro="online" prefix="[" suffix="]"/>
        </group>
      </else-if>
      <else>
        <text macro="online" prefix=" [" suffix="]"/>
      </else>
    </choose>
  </macro>
  <macro name="online">
    <choose>
      <if variable="URL">
        <choose>
          <if type="legislation bill" match="any"/>
          <else>
            <text term="online" text-case="capitalize-first"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <group delimiter=" ">
      <choose>
        <if is-numeric="edition">
          <number variable="edition" form="ordinal" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
          <text term="edition" form="short"/>
        </if>
        <else>
          <text variable="edition" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
        </else>
      </choose>
    </group>
  </macro>
  <macro name="city">
    <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
  </macro>
  <macro name="publisher">
    <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
  </macro>
  <macro name="year">
    <choose>
      <if type="legislation bill" match="any"/>
      <else-if variable="issued accessed" match="none">
        <text term="no date" prefix="[" suffix="]"/>
      </else-if>
      <else-if type="article-newspaper broadcast" match="any">
        <date variable="issued" delimiter=" ">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
          <date-part name="month" form="short" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
          <date-part name="day" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
        </date>
      </else-if>
      <else>
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="accessed">
    <choose>
      <if type="legislation bill" match="any"/>
      <else-if variable="URL">
        <group delimiter=" " prefix="[" suffix="]">
          <text term="accessed"/>
          <date variable="accessed" delimiter=" ">
            <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
            <date-part name="month" form="short" strip-periods="true" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
            <date-part name="day" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
          </date>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="entry">
    <choose>
      <if type="entry-encyclopedia">
        <choose>
          <if variable="author"/>
          <else>
            <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else>
        </choose>
      </if>
      <else-if type="post-weblog">
        <text variable="title" suffix=";&lt;/title&gt;" prefix="&lt;title&gt;"/>
      </else-if>
    </choose>
  </macro>
  <macro name="locator">
    <choose>
      <if type="article-journal article-magazine" match="any">
        <group delimiter=":">
          <group>
            <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
            <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
          </group>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=":">
          <text variable="section" prefix="&lt;section&gt;" suffix="&lt;/section&gt;"/>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
        </group>
      </else-if>
      <else-if type="chapter entry-dictionary entry-encyclopedia" match="any">
        <group delimiter=" " suffix=", ">
          <text term="book" form="short"/>
          <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
        </group>
        <group delimiter=" ">
          <label variable="page" form="short"/>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
        </group>
      </else-if>
      <else-if type="legislation">
        <choose>
          <if variable="number">
            <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
          </if>
          <else>
            <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
          </else>
        </choose>
        <date variable="issued" prefix=" af " delimiter="/">
          <date-part name="day" form="numeric-leading-zeros" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
          <date-part name="month" form="numeric-leading-zeros" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </else-if>
      <else-if type="bill">
        <number variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
        <date variable="issued" prefix=". " delimiter="/">
          <date-part name="day" form="numeric-leading-zeros" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
          <date-part name="month" form="numeric-leading-zeros" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="url">
    <choose>
      <if type="legislation bill" match="none">
        <group delimiter=": ">
          <text term="available at" text-case="capitalize-first"/>
          <text variable="URL" text-decoration="underline" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
        </group>
      </if>
    </choose>
  </macro>
  
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter=", ">
      <text variable="citation-number" prefix="&lt;citation-number&gt;" suffix="&lt;/citation-number&gt;"/>
      <text macro="page" prefix=", "/>
    </layout>
  </citation>
  <bibliography et-al-min="7" et-al-use-first="6" second-field-align="flush">
    <layout>
      <text variable="citation-number" suffix=". &lt;/citation-number&gt;" prefix="&lt;citation-number&gt;"/>
      <group delimiter=". " suffix=".">
        <group delimiter=" ">
          <group delimiter=". ">
            <text macro="author"/>
            <text macro="title"/>
            <text macro="container-title"/>
          </group>
          <text macro="material"/>
        </group>
        <text macro="edition"/>
        <group delimiter="; ">
          <group delimiter=": ">
            <text macro="city"/>
            <text macro="publisher"/>
          </group>
          <group delimiter=" ">
            <group delimiter=". ">
              <text macro="year"/>
              <text macro="entry"/>
            </group>
            <text macro="accessed"/>
          </group>
          <text macro="locator"/>
        </group>
        <text macro="url"/>
      </group>
    </layout>
  </bibliography>
</style>