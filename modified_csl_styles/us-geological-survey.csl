              <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
              <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
            </group>
            <group>
              <text variable="page" suffix=" &lt;/page&gt;" prefix="&lt;page&gt;"/>
              <label variable="page" form="short" plural="never"/>
            </group>
            <group prefix=" ">
              <text term="accessed"/>
              <group prefix=" " suffix=",">
                <date variable="accessed">
                  <date-part name="month" form="long" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
                  <date-part name="day" form="numeric" suffix=", &lt;/day&gt;" prefix="&lt;day&gt;"/>
                  <date-part name="year" form="long" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
                </date>
              </group>
              <group prefix=" ">
                <text term="at"/>
                <text variable="container-title" prefix="&lt;container-title&gt; " suffix="&lt;/container-title&gt;"/>
              </group>
              <group prefix=" ">
                <text term="at"/>
                <text variable="URL" prefix="&lt;URL&gt; " suffix="&lt;/URL&gt;"/>
              </group>
            </group>
          </group>
        </if>
        <else-if variable="version" type="book" match="all">
          <group prefix=" " delimiter=", ">
            
            <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
            <group prefix=" " delimiter=" ">
              <text term="version"/>
              <text variable="version" prefix="&lt;version&gt;" suffix="&lt;/version&gt;"/>
            </group>
            <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
