<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" default-locale="en-US" name-as-sort-order="all" demote-non-dropping-particle="never">
  <info>
    <title>USDA Forest Service - Pacific Northwest Research Station</title>
    <title-short>USFS PNWRS</title-short>
    <id>http://www.zotero.org/styles/usda-forest-service-pacific-northwest-research-station</id>
    <link href="http://www.zotero.org/styles/usda-forest-service-pacific-northwest-research-station" rel="self"/>
    <link href="http://www.zotero.org/styles/harvard-university-of-birmingham" rel="template"/>
    <link href="https://github.com/citation-style-language/styles/files/1321859/UsfsPnwAuthorsGuide2012.pdf" rel="documentation"/>
    <category citation-format="author-date"/>
    <category field="geography"/>
    <category field="botany"/>
    <updated>2017-05-24T19:10:10+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="editor">
    <names variable="editor" delimiter="; " prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name delimiter="; " delimiter-precedes-last="never" initialize-with="." name-as-sort-order="all"/>
      <label form="short" strip-periods="false" prefix=", " suffix="."/>
    </names>
  </macro>
  <macro name="cite-editor">
    <names variable="editor" delimiter=", " prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name form="short" and="text" delimiter=", " delimiter-precedes-last="never" initialize-with=". "/>
    </names>
  </macro>
  <macro name="bookauthor">
    <names variable="container-author" prefix="&lt;container-author&gt;" suffix="&lt;/container-author&gt;">
      <name name-as-sort-order="all" sort-separator=", " initialize-with="." delimiter="; "/>
      <substitute>
        <text macro="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="director">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name name-as-sort-order="all" sort-separator=", " initialize-with="." delimiter=", "/>
      <substitute>
        <text macro="editor"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <choose>
      <if type="broadcast motion_picture" match="any">
        <choose>
          <if variable="container-title">
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </if>
          <else-if variable="title">
            <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else-if>
        </choose>
      </if>
      <else-if type="bill legislation" match="any">
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else>
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name form="short" and="text" delimiter-precedes-last="never" initialize-with=". " name-as-sort-order="all"/>
          <substitute>
            <text macro="cite-editor"/>
            <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
            <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="author">
    <choose>
      <if type="broadcast motion_picture" match="any">
        <choose>
          <if variable="container-title">
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </if>
          <else-if variable="title">
            <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else-if>
        </choose>
      </if>
      <else-if type="bill legislation" match="any">
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else>
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name delimiter="; " initialize-with="." name-as-sort-order="all"/>
          <label form="short" prefix=" " strip-periods="false"/>
          <substitute>
            <text macro="editor"/>
            <choose>
              <if type="webpage">
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
              </if>
              <else-if variable="container-title">
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
              </else-if>
              <else>
                <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
              </else>
            </choose>
          </substitute>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
  </macro>
  <macro name="publisher">
    <group delimiter=": ">
      <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
      <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
    </group>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short" text-case="capitalize-first" prefix="[" suffix="]"/>
      </else>
    </choose>
  </macro>
  <macro name="year-date-cite">
    <choose>
      <if variable="issued">
        <date variable="issued" prefix=" ">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short" prefix=", "/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short" strip-periods="false"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix=".&lt;/edition&gt;" prefix="&lt;edition&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if type="article-journal article-magazine chapter" match="any">
        <choose>
          <if variable="volume issue" match="any" type="chapter">
            <text variable="page" prefix="&lt;page&gt;: " suffix=".&lt;/page&gt;"/>
          </if>
          <else>
            <label variable="page" form="short" suffix=" "/>
            <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
          </else>
        </choose>
      </if>
      <else>
        <group>
          <text variable="page" form="short" strip-periods="false" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
          <label strip-periods="true" prefix=" " variable="page" form="short"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="volumes">
    <group>
      <text variable="number-of-volumes" suffix=" &lt;/number-of-volumes&gt;" prefix="&lt;number-of-volumes&gt;"/>
      <label variable="volume" form="short" plural="always"/>
    </group>
  </macro>
  <macro name="online">
    <choose>
      <if variable="URL">
        <text variable="URL" suffix=" &lt;/URL&gt;" prefix="&lt;URL&gt;"/>
      </if>
    </choose>
    <choose>
      <if match="any" variable="accessed">
        <date form="text" variable="accessed" prefix="&lt;accessed&gt;(" suffix=").&lt;/accessed&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="issued">
    <choose>
      <if type="article-newspaper article-magazine motion_picture paper-conference broadcast" match="any">
        <date variable="issued">
          <date-part name="day" suffix=" &lt;/day&gt;" prefix="&lt;day&gt;"/>
          <date-part name="month" prefix="&lt;month&gt;" suffix="&lt;/month&gt;"/>
        </date>
      </if>
    </choose>
    <choose>
      <if type="motion_picture broadcast paper-conference" match="any">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt; " suffix="&lt;/year&gt;"/>
        </date>
      </if>
    </choose>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" name-as-sort-order="all" disambiguate-add-year-suffix="true" collapse="year">
    <sort>
      <key macro="author"/>
      <key macro="year-date" sort="ascending"/>
      <key variable="title"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <group>
        <text macro="author-short"/>
        <choose>
          <if type="bill legislation" match="none">
            <text macro="year-date-cite"/>
          </if>
        </choose>
        <group>
          <label variable="locator" form="short" strip-periods="false"/>
          <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
        </group>
      </group>
    </layout>
  </citation>
  <bibliography hanging-indent="false">
    <sort>
      <key macro="author"/>
      <key macro="year-date" sort="ascending"/>
      <key variable="title"/>
    </sort>
    <layout>
      <group font-style="normal" font-variant="normal" font-weight="bold" text-decoration="none">
        <text macro="author"/>
        <text macro="year-date" prefix=" " suffix="."/>
      </group>
      <group>
        <choose>
          <if type="bill legislation" match="any">
            <group delimiter=". ">
              <text variable="container-title" font-weight="bold" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
              <text macro="edition"/>
              <text macro="editor"/>
              <text variable="number" suffix=")&lt;/number&gt;" prefix="&lt;number&gt;("/>
              <text variable="note" prefix="&lt;note&gt;" suffix="&lt;/note&gt;"/>
            </group>
          </if>
          <else-if type="thesis">
            <group delimiter=". ">
              <group prefix=" " delimiter=" ">
                <text macro="title"/>
              </group>
              <text macro="publisher"/>
              <text macro="pages"/>
              <text variable="genre" suffix=". &lt;/genre&gt;" prefix="&lt;genre&gt;"/>
            </group>
          </else-if>
          <else-if type="webpage">
            <group delimiter=". " prefix=" ">
              <text macro="title"/>
              <text macro="online"/>
              <text macro="edition"/>
            </group>
          </else-if>
          <else-if type="manuscript">
            <group delimiter=". ">
              <group delimiter=" ">
                <text macro="title"/>
                <text variable="genre" prefix="&lt;genre&gt;[" suffix="]&lt;/genre&gt;"/>
              </group>
              <text variable="archive" prefix="&lt;archive&gt;" suffix="&lt;/archive&gt;"/>
              <text variable="archive_location" prefix="&lt;archive_location&gt;" suffix="&lt;/archive_location&gt;"/>
              <text macro="online"/>
              <text macro="publisher"/>
            </group>
          </else-if>
          <else-if type="article-journal article-magazine article-newspaper interview map patent personal_communication song speech" match="any">
            <group>
              <group suffix=".">
                <text macro="title" prefix=" "/>
                <text macro="editor" prefix=" "/>
              </group>
              <group delimiter=" " prefix=" ">
                <text variable="container-title" suffix=". &lt;/container-title&gt;" prefix="&lt;container-title&gt;"/>
                <text macro="online"/>
              </group>
              <group>
                <text macro="issued"/>
                <group>
                  <group>
                    <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                    <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
                  </group>
                  <text macro="pages"/>
                </group>
              </group>
            </group>
          </else-if>
          <else-if type="broadcast motion_picture" match="any">
            <group delimiter=". ">
              <group delimiter=", ">
                <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
                <choose>
                  <if variable="container-title">
                    <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                  </if>
                </choose>
              </group>
              <group delimiter=" ">
                <choose>
                  <if type="motion_picture" match="any">
                    <text value="Film"/>
                  </if>
                </choose>
                <text macro="online"/>
              </group>
              <group delimiter=". ">
                <text macro="director"/>
                <text macro="publisher"/>
                <text macro="issued"/>
              </group>
            </group>
          </else-if>
          <else-if type="bill book graphic legal_case report" match="any">
            <group delimiter=". " suffix=".">
              <group prefix=" ">
                <text macro="title"/>
                <group delimiter=" " prefix=". ">
                  <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                  <text variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
                  <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
                  <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
                </group>
                <group delimiter=". " prefix=". ">
                  <text macro="volumes"/>
                  <text macro="edition"/>
                  <text macro="editor"/>
                </group>
              </group>
              <text macro="publisher" prefix=" "/>
              <text macro="pages"/>
            </group>
          </else-if>
          <else-if type="chapter" match="any">
            <group>
              <text macro="title" prefix=" " suffix="."/>
              <group prefix=" " delimiter=" ">
                <text term="in" text-case="capitalize-first" text-decoration="none" suffix=": "/>
                <text macro="bookauthor"/>
                <group prefix=" " suffix=". ">
                  <text variable="container-title" font-weight="normal" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
                  <group delimiter=" " prefix=". ">
                    <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                    <text variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
                    <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
                  </group>
                  <group delimiter=". " prefix=". ">
                    <text macro="volumes"/>
                    <text macro="edition"/>
                  </group>
                </group>
              </group>
            </group>
            <text macro="publisher"/>
            <text macro="pages"/>
            <text macro="online" prefix=" "/>
          </else-if>
          <else-if type="paper-conference" match="any">
            <group>
              <text macro="title" prefix=" " suffix="."/>
              <group delimiter=". ">
                <group prefix=" " delimiter=" ">
                  <text term="in" text-case="capitalize-first" suffix=" " text-decoration="underline"/>
                  <text macro="editor" suffix="."/>
                  <text variable="container-title" font-weight="bold" suffix=".&lt;/container-title&gt;" prefix="&lt;container-title&gt;"/>
                  <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
                  <group delimiter=". ">
                    <text variable="publisher-place" font-weight="bold" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
                    <text macro="issued" font-weight="bold"/>
                  </group>
                  <text macro="online"/>
                </group>
                <group delimiter=". ">
                  <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
                  <text macro="pages"/>
                </group>
              </group>
            </group>
          </else-if>
          <else>
            <group delimiter=". ">
              <group>
                <text macro="title" prefix=" "/>
                <text macro="editor" prefix=" "/>
              </group>
              <group delimiter=" ">
                <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
              </group>
              <group prefix=" ">
                <group prefix=" ">
                  <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
                  <text variable="issue" prefix="&lt;issue&gt; (" suffix=")&lt;/issue&gt;"/>
                  <text macro="issued"/>
                </group>
                <group prefix=" ">
                  <text macro="pages"/>
                </group>
              </group>
            </group>
          </else>
        </choose>
      </group>
    </layout>
  </bibliography>
</style>