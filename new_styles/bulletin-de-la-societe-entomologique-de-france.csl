  </info>
  <locale>
    <terms>
      <term name="in">dans</term>
      <term name="from">de</term>
      <term name="retrieved">récupéré</term>
      <term name="accessed">consulté le</term>
    </terms>
  </locale>
  <macro name="author">
    <names variable="author" font-variant="small-caps" suffix=".&lt;/author&gt;" prefix="&lt;author&gt;">
      <name font-variant="normal" suffix="." and="symbol" delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="19" et-al-use-first="15" initialize-with=". " name-as-sort-order="all" sort-separator=" ">
        <name-part name="family" text-case="capitalize-first" font-variant="small-caps"/>
      </name>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" suffix=". &lt;/editor&gt;" prefix="&lt;editor&gt;">
      <name sort-separator=" " initialize-with="" name-as-sort-order="all" delimiter=", " delimiter-precedes-last="always"/>
    </names>
  </macro>
  <macro name="publisher">
    <group delimiter=" : ">
      <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
      <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
    </group>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI">
        <text variable="DOI" prefix="&lt;DOI&gt; doi : " suffix="&lt;/DOI&gt;"/>
      </if>
      <else-if type="webpage" match="any">
        <group delimiter=" ">
          <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
          <group>
            <text term="accessed" text-case="capitalize-first" suffix=" "/>
            <date variable="accessed">
              <date-part name="day" suffix="/&lt;/day&gt;" prefix="&lt;day&gt;"/>
              <date-part name="month" form="numeric" text-case="lowercase" suffix="/&lt;/month&gt;" prefix="&lt;month&gt;"/>
              <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
            </date>
          </group>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="title">
    <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short" suffix="." strip-periods="true"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix=".&lt;/edition&gt;" prefix="&lt;edition&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="point-locators">
    <group>
      <choose>
        <if locator="page" match="none">
          <label variable="locator" form="short" suffix=" "/>
        </if>
      </choose>
      <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
    </group>
  </macro>
  <macro name="contributors-short">
    <names variable="author" suffix=",&lt;/author&gt;" prefix="&lt;author&gt;">
      <name form="short" font-variant="small-caps" and="symbol" initialize="false" sort-separator=" ; "/>
      <et-al font-style="italic" font-variant="normal"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <text macro="title"/>
      </substitute>
    </names>
  </macro>
  <macro name="date-reference">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short" text-case="lowercase"/>
      </else>
    </choose>
  </macro>
  <citation name-form="short" and="text" et-al-min="3" et-al-use-first="1" disambiguate-add-names="false" disambiguate-add-givenname="false" disambiguate-add-year-suffix="true">
    <sort>
      <key variable="issued" sort="ascending"/>
      <key macro="author"/>
    </sort>
    <layout delimiter=" ; " prefix="(" suffix=")">
      <group delimiter=":">
        <group delimiter=" ">
          <text macro="contributors-short"/>
          <text macro="date-reference"/>
        </group>
        <text macro="point-locators"/>
      </group>
    </layout>
  </citation>
  <bibliography delimiter-precedes-et-al="never" delimiter-precedes-last="never" et-al-min="12" et-al-use-first="6" subsequent-author-substitute="–––">
    <sort>
      <key macro="author"/>
      <key variable="issued"/>
    </sort>
    <layout suffix=".">
      <text macro="author" suffix=", "/>
      <date date-parts="year" form="text" variable="issued" suffix=". – ">
        <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
      </date>
      <choose>
        <if type="bill book graphic legal_case report thesis" match="any">
          <text macro="title" font-style="italic" suffix="."/>
          <text macro="edition" prefix=" " suffix=","/>
          <text macro="publisher" prefix=" "/>
          <text variable="number-of-pages" prefix="&lt;number-of-pages&gt;, " suffix=" p.&lt;/number-of-pages&gt;"/>
          <text variable="page" prefix="&lt;page&gt;, " suffix=" p.&lt;/page&gt;"/>
        </if>
        <else-if type="chapter paper-conference" match="any">
          <text macro="title" font-style="italic" suffix=". "/>
          <group suffix=". " delimiter=" ">
            <text term="in" text-case="capitalize-first" suffix=" :"/>
            <text macro="editor"/>
            <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          </group>
          <text macro="publisher" prefix=" "/>
          <text variable="page" prefix="&lt;page&gt;, p. " suffix=".&lt;/page&gt;"/>
        </else-if>
        <else>
          <text macro="title" suffix=". "/>
          <text variable="container-title" font-style="italic" suffix=", &lt;/container-title&gt;" prefix="&lt;container-title&gt;"/>
          <text variable="volume" font-weight="bold" suffix=" &lt;/volume&gt;" prefix="&lt;volume&gt;"/>
          <text variable="issue" prefix="&lt;issue&gt;(" suffix=") &lt;/issue&gt;"/>
          <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
        </else>
      </choose>
      <text macro="access"/>
    </layout>
  </bibliography>
</style>