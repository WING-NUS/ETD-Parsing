<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="note" default-locale="fr-FR" version="1.0" delimiter-precedes-last="never" delimiter-precedes-et-al="never" and="text">
  <info>
    <title>infoclio.ch (petites majuscules, French)</title>
    <id>http://www.zotero.org/styles/infoclio-fr-smallcaps</id>
    <link href="http://www.zotero.org/styles/infoclio-fr-smallcaps" rel="self"/>
    <link href="https://www.infoclio.ch/fr/node/138218" rel="documentation"/>
    <author>
      <name>Nicolas Chachereau</name>
      <email>nicolas.chachereau@unil.ch</email>
    </author>
    <author>
      <name>Enrico Natale</name>
      <email>enrico.natale@infoclio.ch</email>
    </author>
    <author>
      <name>Jan Baumann</name>
      <email>jan.baumann@infoclio.ch</email>
    </author>
    <contributor>
      <name>Jonas Schneider</name>
      <email>jonas.schneider@uzh.ch</email>
    </contributor>
    <category citation-format="note"/>
    <category field="history"/>
    <category field="social_science"/>
    <updated>2018-03-27T11:57:56+02:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="fr">
    <date form="numeric">
      <date-part name="day" form="numeric-leading-zeros" suffix="."/>
      <date-part name="month" form="numeric-leading-zeros" suffix="."/>
      <date-part name="year"/>
    </date>
    <terms>
      <term name="cited" form="short">cit.</term>
      <term name="page" form="short">
        <single>p.</single>
        <multiple>pp.</multiple>
      </term>
      <term name="editor" form="short">
        <single>éd.</single>
        <multiple>éds.</multiple>
      </term>
      <term name="interviewer" form="verb">entretien réalisé par</term>
      <term name="letter">courrier</term>
      <term name="number-of-volumes" form="short">vol.</term>
    </terms>
  </locale>
  <citation>
    <layout suffix="." delimiter=" ; ">
      <choose>
        <if position="ibid-with-locator">
          <group delimiter=", ">
            <text term="ibid"/>
            <text macro="locator"/>
          </group>
        </if>
        <else-if position="ibid">
          <text term="ibid"/>
        </else-if>
        <else-if position="subsequent">
          <text macro="subsequent-reference"/>
        </else-if>
        <else>
          <text macro="complete-reference"/>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography>
    <sort>
      <key macro="creator" names-min="3" names-use-first="3"/>
      <key variable="issued" sort="descending"/>
    </sort>
    <layout suffix=".">
      <text macro="complete-reference"/>
    </layout>
  </bibliography>
  <macro name="complete-reference">
    <group delimiter=". ">
      <group delimiter=", ">
        <text macro="creator"/>
        <text macro="title"/>
        <group delimiter=": ">
          <text macro="in"/>
          <group delimiter=", ">
            <text macro="container-creator"/>
            <group delimiter=" ">
              <text macro="container-information"/>
              <text macro="journal-volume"/>
            </group>
            <text macro="volumes"/>
          </group>
        </group>
        <text macro="type-description"/>
        <text macro="alt-publisher"/>
        <text macro="edition"/>
        <text macro="place"/>
        <text macro="publishing-house"/>
        <group delimiter=" ">
          <text macro="date"/>
          <date variable="original-date" form="text" prefix="&lt;original-date&gt;[" suffix="]&lt;/original-date&gt;"/>
          <text macro="book-series"/>
        </group>
        <text macro="artwork-description"/>
        <text macro="archive-location"/>
        <text macro="pages"/>
        <text macro="url-web-documents-only"/>
      </group>
      <text macro="url-non-web-documents"/>
    </group>
  </macro>
  <macro name="subsequent-reference">
    <group delimiter=", ">
      <text macro="creator-for-subsequent"/>
      <text macro="identifier-for-subsequent"/>
      <text macro="op-cit"/>
      <date variable="issued" form="numeric" date-parts="year" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
      <text macro="locator"/>
    </group>
  </macro>
  <macro name="creator">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name sort-separator=" " name-as-sort-order="all" et-al-min="4" et-al-use-first="3">
        <name-part name="family" font-variant="small-caps"/>
      </name>
      <label form="short" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="composer" prefix="&lt;composer&gt;" suffix="&lt;/composer&gt;"/>
        <names variable="director" prefix="&lt;director&gt;" suffix="&lt;/director&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="creator-for-subsequent">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" et-al-min="4" et-al-use-first="1">
        <name-part name="family" font-variant="small-caps"/>
      </name>
      <label form="short" prefix=" (" suffix=")"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="composer" prefix="&lt;composer&gt;" suffix="&lt;/composer&gt;"/>
        <names variable="director" prefix="&lt;director&gt;" suffix="&lt;/director&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="title">
    <choose>
      <if type="book thesis graphic" match="any">
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else-if type="article                      article-magazine article-newspaper article-journal                      entry entry-dictionary entry-encyclopedia                      report chapter paper-conference                      post post-weblog webpage                      song broadcast" match="any">
        <text variable="title" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else-if>
      <else-if type="motion_picture">
        <choose>
          <if variable="collection-title">
            <text variable="title" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </if>
          <else>
            <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="identifier-for-subsequent">
    <choose>
      <if variable="title title-short" match="any">
        <choose>
          <if type="book thesis graphic" match="any">
            <text variable="title" font-style="italic" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </if>
          <else-if type="article                          article-magazine article-newspaper article-journal                          entry entry-dictionary entry-encyclopedia                          report chapter paper-conference                          post post-weblog webpage                          song broadcast" match="any">
            <text variable="title" quotes="true" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else-if>
          <else-if type="motion_picture">
            <choose>
              <if variable="collection-title">
                <text variable="title" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
              </if>
              <else>
                <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
              </else>
            </choose>
          </else-if>
          <else>
            <text variable="title" form="short" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else>
        </choose>
      </if>
      <else-if type="personal_communication">
        <group delimiter=" ">
          <text term="letter"/>
          <names variable="recipient" prefix="&lt;recipient&gt;" suffix="&lt;/recipient&gt;">
            <label form="verb" prefix=" " suffix=" "/>
            <name et-al-min="3" et-al-use-first="1"/>
          </names>
        </group>
      </else-if>
      <else-if type="interview">
        <names variable="interviewer" delimiter=", " prefix="&lt;interviewer&gt;" suffix="&lt;/interviewer&gt;">
          <label form="verb" prefix=" " suffix=" "/>
          <name et-al-min="3" et-al-use-first="1"/>
        </names>
      </else-if>
      <else-if type="report song broadcast motion_picture                      webpage post post-weblog" match="any">
        
        <text variable="collection-title" font-style="italic" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
        <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </else-if>
    </choose>
  </macro>
  <macro name="op-cit">
    <group font-style="italic" delimiter=" " suffix=".">
      <choose>
        <if type="article                   article-magazine article-newspaper article-journal                   entry entry-dictionary entry-encyclopedia                   chapter paper-conference                   post post-weblog" match="any">
          <text value="art."/>
        </if>
        <else-if type="manuscript personal_communication interview                        report webpage" match="any">
          <text value="doc."/>
        </else-if>
        <else-if type="book thesis graphic                        motion_picture song broadcast" match="any">
          <text value="op."/>
        </else-if>
      </choose>
      <text term="cited" form="short"/>
    </group>
  </macro>
  <macro name="in">
    <choose>
      <if type="chapter paper-conference                 entry-encyclopedia entry-dictionary" match="any">
        <text term="in"/>
      </if>
    </choose>
  </macro>
  <macro name="container-creator">
    <choose>
      <if type="chapter paper-conference" match="any">
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
          <name sort-separator=" " name-as-sort-order="all" et-al-min="4" et-al-use-first="3">
            <name-part name="family" font-variant="small-caps"/>
          </name>
          <label form="short" prefix=" (" suffix=")"/>
          <substitute>
            <names variable="container-author" prefix="&lt;container-author&gt;" suffix="&lt;/container-author&gt;"/>
            <names variable="collection-editor" prefix="&lt;collection-editor&gt;" suffix="&lt;/collection-editor&gt;"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="container-information">
    <choose>
      <if type="chapter paper-conference                 entry-encyclopedia entry-dictionary                 article-newspaper article-magazine article-journal" match="any">
        <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      </if>
      <else-if type="report song broadcast motion_picture                      webpage post post-weblog" match="any">
        <group delimiter=", ">
          <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
          
          <text variable="collection-title" font-style="italic" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
          <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="journal-volume">
    <choose>
      <if type="article-journal">
        <group delimiter=" " prefix="&lt;issue&gt;&lt;volume&gt;" suffix="&lt;/volume&gt;&lt;/issue&gt;">
          <number variable="volume"/>
          <number variable="issue" prefix="(" suffix=")"/>
        </group>
      </if>
      <else-if type="report song broadcast" match="any">
        <number variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
      </else-if>
    </choose>
  </macro>
  <macro name="volumes">
    <choose>
      <if type="book chapter                 entry-encyclopedia entry-dictionary                 song motion_picture" match="any">
        <group delimiter=" / ">
          <group delimiter=" " prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;">
            <label variable="volume" form="short"/>
            <number variable="volume"/>
          </group>
          <group delimiter=" " prefix="&lt;number-of-volumes&gt;" suffix="&lt;/number-of-volumes&gt;">
            <number variable="number-of-volumes"/>
            <choose>
              <if variable="volume" match="none">
                <label variable="number-of-volumes" form="short"/>
              </if>
            </choose>
          </group>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="type-description">
    <choose>
      <if type="manuscript thesis speech" match="any">
        <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
      </if>
      <else-if type="personal_communication">
        <group delimiter=", ">
          <group delimiter=" ">
            <text term="letter"/>
            <names variable="recipient" prefix="&lt;recipient&gt;" suffix="&lt;/recipient&gt;">
              <label form="verb" prefix=" " suffix=" "/>
              <name/>
            </names>
          </group>
          <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
        </group>
      </else-if>
      <else-if type="interview">
        <names variable="interviewer" delimiter=", " prefix="&lt;interviewer&gt;" suffix="&lt;/interviewer&gt;">
          <label form="verb" prefix=" " suffix=" "/>
          <name/>
        </names>
      </else-if>
      <else-if type="motion_picture song broadcast" match="any">
        <group delimiter=", ">
          <text variable="medium" prefix="&lt;medium&gt;" suffix="&lt;/medium&gt;"/>
          
          <text variable="dimensions" prefix="&lt;dimensions&gt;" suffix="&lt;/dimensions&gt;"/>
        </group>
      </else-if>
      
      <else-if type="book">
        <choose>
          <if variable="version medium" match="any">
            <group delimiter=", ">
              <group delimiter=" ">
                <text term="version"/>
                <text variable="version" prefix="&lt;version&gt;" suffix="&lt;/version&gt;"/>
              </group>
              <text variable="medium" prefix="&lt;medium&gt;" suffix="&lt;/medium&gt;"/>
            </group>
          </if>
        </choose>
      </else-if>
    </choose>
  </macro>
  <macro name="alt-publisher">
    <choose>
      <if type="book chapter                 thesis report                 paper-conference                 entry-dictionary entry-encyclopedia" match="none">
        
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if type="book chapter                 entry-dictionary entry-encyclopedia" match="any">
        <choose>
          <if is-numeric="edition">
            <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
              <number variable="edition" form="ordinal"/>
              <label variable="edition"/>
            </group>
          </if>
          <else>
            <text variable="edition" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="place">
    <choose>
      <if type="speech">
        <group delimiter=", ">
          <text variable="event" prefix="&lt;event&gt;" suffix="&lt;/event&gt;"/>
          <text variable="event-place" prefix="&lt;event-place&gt;" suffix="&lt;/event-place&gt;"/>
        </group>
      </if>
      <else>
        <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="publishing-house">
    <choose>
      <if type="book chapter thesis report                 paper-conference                 entry-dictionary entry-encyclopedia" match="any">
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if type="book chapter paper-conference thesis" match="any">
        <choose>
          <if variable="issued">
            <date variable="issued" form="numeric" date-parts="year" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
          </if>
          <else>
            <text term="no date" form="short"/>
          </else>
        </choose>
      </if>
      <else-if type="article-journal article-newspaper article-magazine                      graphic entry-encyclopedia entry-dictionary                      report speech interview                      manuscript personal_communication" match="any">
        <choose>
          <if variable="issued">
            <date variable="issued" form="numeric" date-parts="year-month-day" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
          </if>
          <else>
            <text term="no date" form="short"/>
          </else>
        </choose>
      </else-if>
      <else>
        <date variable="issued" form="numeric" date-parts="year-month-day" prefix="&lt;issued&gt;" suffix="&lt;/issued&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <choose>
      <if variable="locator">
        <text macro="locator"/>
      </if>
      <else>
        <group delimiter=" ">
          <label variable="page" form="short"/>
          <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="locator">
    <group delimiter=" ">
      <label variable="locator" form="short"/>
      <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
    </group>
  </macro>
  <macro name="book-series">
    <choose>
      <if type="book chapter paper-conference                 entry-dictionary entry-encyclopedia" match="any">
        <group prefix="(" suffix=")" delimiter=" ">
          <text variable="collection-title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
          <choose>
            <if is-numeric="collection-number">
              <number variable="collection-number" prefix="&lt;collection-number&gt;" suffix="&lt;/collection-number&gt;"/>
            </if>
          </choose>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="artwork-description">
    <choose>
      <if type="graphic">
        <group delimiter=", ">
          <text variable="medium" prefix="&lt;medium&gt;" suffix="&lt;/medium&gt;"/>
          <text variable="dimensions" prefix="&lt;dimensions&gt;" suffix="&lt;/dimensions&gt;"/>
          <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="archive-location">
    <choose>
      <if variable="archive">
        <group delimiter=", ">
          <text variable="archive" prefix="&lt;archive&gt;" suffix="&lt;/archive&gt;"/>
          <text variable="archive_location" prefix="&lt;archive_location&gt;" suffix="&lt;/archive_location&gt;"/>
          <text variable="call-number" prefix="&lt;call-number&gt;Cote: " suffix="&lt;/call-number&gt;"/>
        </group>
      </if>
    </choose>
  </macro>
  <macro name="url-web-documents-only">
    <choose>
      <if type="webpage post post-weblog" match="any">
        <text macro="url"/>
      </if>
    </choose>
  </macro>
  <macro name="url-non-web-documents">
    <choose>
      <if type="webpage post post-weblog" match="none">
        <choose>
          <if variable="DOI URL" match="any">
            <group delimiter=" ">
              <text term="online" text-case="capitalize-first" suffix=": "/>
              <choose>
                <if variable="DOI">
                  <group delimiter=", ">
                    <group delimiter="">
                      <text value="&lt;"/>
                      <text variable="DOI" prefix="&lt;DOI&gt;https://doi.org/" suffix="&lt;/DOI&gt;"/>
                      <text value="&gt;"/>
                    </group>
                    <group delimiter=" ">
                      <text term="accessed"/>
                      <date variable="accessed" form="numeric" date-parts="year-month-day" prefix="&lt;accessed&gt;" suffix="&lt;/accessed&gt;"/>
                    </group>
                  </group>
                </if>
                <else-if variable="URL">
                  <text macro="url"/>
                </else-if>
              </choose>
            </group>
          </if>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="url">
    <group delimiter=", ">
      <text variable="URL" prefix="&lt;URL&gt;&lt;" suffix="&gt;&lt;/URL&gt;"/>
      <group delimiter=" ">
        <text term="accessed"/>
        <date variable="accessed" form="numeric" date-parts="year-month-day" prefix="&lt;accessed&gt;" suffix="&lt;/accessed&gt;"/>
      </group>
    </group>
  </macro>
</style>