<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0" default-locale="en-GB" class="in-text">
  <info>
    <title>International Journal of Spatial Data Infrastructures Research</title>
    <title-short>IJSDIR</title-short>
    <id>http://www.zotero.org/styles/international-journal-of-spatial-data-infrastructures-research</id>
    <link href="http://www.zotero.org/styles/international-journal-of-spatial-data-infrastructures-research" rel="self"/>
    <link href="http://ijsdir.jrc.ec.europa.eu/index.php/ijsdir/pages/view/authors" rel="documentation"/>
    <author>
      <name>Lorenzino Vaccari</name>
      <email>lorenzino.vaccari@gmail.com</email>
    </author>
    <contributor>
      <name>Elena Roglia</name>
    </contributor>
    <category citation-format="author-date"/>
    <category field="science"/>
    <issn>1725-0463</issn>
    <summary>The International Journal of Spatial Data Infrastructures Research style.</summary>
    <updated>2016-06-22T00:00:00+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="contributors-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="text" delimiter=", "/>
    </names>
  </macro>
  <macro name="author">
    <choose>
      <if type="webpage">
        <choose>
          <if variable="author">
            <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
              <name name-as-sort-order="first" delimiter-precedes-last="never" and="text"/>
            </names>
          </if>
          <else>
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          </else>
        </choose>
      </if>
      <else>
        <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
          <name name-as-sort-order="first" initialize-with="." delimiter-precedes-last="never" and="text"/>
        </names>
      </else>
    </choose>
  </macro>
  <macro name="issued">
    <date variable="issued">
      <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <macro name="title">
    <choose>
      <if type="article-journal">
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        <text variable="container-title" prefix="&lt;container-title&gt;, " font-style="italic" suffix="&lt;/container-title&gt;"/>
        <group prefix=", ">
          <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
          <text variable="issue" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
          <choose>
            <if variable="URL">
              <text variable="URL" prefix="&lt;URL&gt;, at " suffix="&lt;/URL&gt;"/>
              <text macro="accessed"/>
            </if>
            <else>
              <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
            </else>
          </choose>
        </group>
      </if>
      <else-if type="book">
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        <text macro="publisher" prefix=", "/>
      </else-if>
      <else-if type="chapter">
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" prefix="&lt;title&gt;&quot;" suffix="&quot;&lt;/title&gt;"/>
        <text macro="editor"/>
        <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
        <text macro="publisher" prefix=", "/>
        <text macro="page" prefix=", "/>
      </else-if>
      <else-if type="paper-conference">
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" prefix="&lt;title&gt;&quot;" suffix="&quot;&lt;/title&gt;"/>
        <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;, " suffix="&lt;/container-title&gt;"/>
        <choose>
          <if variable="URL">
            <text variable="URL" prefix="&lt;URL&gt;, at " suffix="&lt;/URL&gt;"/>
            <text macro="accessed"/>
          </if>
          <else>
            <text macro="page" prefix=", "/>
            <text macro="publisher" prefix=", "/>
          </else>
        </choose>
      </else-if>
      <else-if type="report">
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" font-style="italic" suffix=", &lt;/title&gt;" prefix="&lt;title&gt;"/>
        <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
        <text macro="publisher" prefix=", "/>
      </else-if>
      <else-if type="thesis">
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        <text variable="genre" prefix="&lt;genre&gt;, " suffix="&lt;/genre&gt;"/>
        <choose>
          <if variable="URL">
            <text variable="URL" prefix="&lt;URL&gt;, at " suffix="&lt;/URL&gt;"/>
            <text macro="accessed"/>
          </if>
        </choose>
        <text macro="publisher" prefix=", "/>
      </else-if>
      <else-if type="webpage">
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
        <text variable="URL" prefix="&lt;URL&gt;, at " suffix="&lt;/URL&gt;"/>
        <text macro="accessed"/>
      </else-if>
      <else>
        <text macro="issued" prefix="(" suffix="). "/>
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="accessed">
    <choose>
      <if variable="accessed">
        <date variable="accessed" prefix=", [accessed " suffix="]">
          <date-part name="day" suffix=" &lt;/day&gt;" range-delimiter="-" prefix="&lt;day&gt;"/>
          <date-part name="month" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
    </choose>
  </macro>
  <macro name="page">
    <label variable="page" form="short"/>
    <text variable="page" prefix="&lt;page&gt;. " suffix="&lt;/page&gt;"/>
  </macro>
  <macro name="publisher">
    <text variable="publisher-place" suffix=": &lt;/publisher-place&gt;" prefix="&lt;publisher-place&gt;"/>
    <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
  </macro>
  <macro name="editor">
    <names variable="editor" delimiter=", " suffix=". &lt;/editor&gt;" prefix="&lt;editor&gt;, in ">
      <name and="text" name-as-sort-order="first" initialize-with=". "/>
      <label form="short" prefix=" (" text-case="title" suffix=")"/>
    </names>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-names="true" disambiguate-add-givenname="true" givenname-disambiguation-rule="primary-name">
    <layout prefix="(" suffix=")" delimiter="; ">
      <text macro="contributors-short"/>
      <text macro="issued" prefix=", "/>
    </layout>
  </citation>
  <bibliography>
    <sort>
      <key macro="author"/>
      <key macro="issued"/>
    </sort>
    <layout suffix=".">
      <group delimiter=" ">
        <text macro="author"/>
        <text macro="title"/>
      </group>
    </layout>
  </bibliography>
</style>