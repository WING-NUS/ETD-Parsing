          <text variable="page" suffix=".&lt;/page&gt;" prefix="&lt;page&gt;"/>
        </else-if>
        <else-if type="webpage">
          <text variable="URL" prefix="&lt;URL&gt;Available at: " suffix=". &lt;/URL&gt;"/>
        </else-if>
        <else>
          <group suffix=". ">
            <text macro="title"/>
            <text variable="container-title" font-style="italic" suffix=", &lt;/container-title&gt;" form="long" prefix="&lt;container-title&gt;"/>
            <text variable="volume" font-weight="bold" suffix=", &lt;/volume&gt;" prefix="&lt;volume&gt;"/>
            <text variable="page" suffix=".&lt;/page&gt;" prefix="&lt;page&gt;"/>
          </group>
