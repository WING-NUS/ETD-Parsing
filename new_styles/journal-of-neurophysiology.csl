<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-US">
  <info>
    <title>Journal of Neurophysiology</title>
    <id>http://www.zotero.org/styles/journal-of-neurophysiology</id>
    <link href="http://www.zotero.org/styles/journal-of-neurophysiology" rel="self"/>
    <link href="http://www.zotero.org/styles/american-physics-society" rel="template"/>
    <link href="http://www.the-aps.org/mm/Publications/Info-For-Authors/Composition#references" rel="documentation"/>
    <author>
      <name>Michael Pesavento</name>
      <email>mpesaven@gmail.com</email>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <category field="medicine"/>
    <issn>0022-3077</issn>
    <eissn>1522-1598</eissn>
    <summary>The Journal of Neurophysiology style</summary>
    <updated>2016-05-04T16:21:17+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <locale xml:lang="en">
    <terms>
      <term name="no date" form="long">date unknown</term>
    </terms>
  </locale>
  <macro name="anon">
    <text term="anonymous" form="short" text-case="capitalize-first" strip-periods="true"/>
  </macro>
  <macro name="author-bib">
    <names variable="author" suffix=". &lt;/author&gt;" prefix="&lt;author&gt;">
      <name sort-separator=" " initialize-with="" name-as-sort-order="all" delimiter=", " font-weight="bold" delimiter-precedes-last="always"/>
      <label form="long" prefix=", " suffix="."/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-cit">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="text" delimiter=" " initialize-with=""/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <text macro="anon"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;edited by " suffix="&lt;/editor&gt;">
      <name sort-separator=" " initialize-with="" name-as-sort-order="all" delimiter=", " delimiter-precedes-last="always"/>
    </names>
  </macro>
  <macro name="publisher">
    <choose>
      <if variable="URL">
        <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      </if>
      <else>
        <text variable="publisher-place" suffix=": &lt;/publisher-place&gt;" prefix="&lt;publisher-place&gt;"/>
        <text variable="publisher" suffix=", &lt;/publisher&gt;" prefix="&lt;publisher&gt;"/>
        <text macro="date"/>
      </else>
    </choose>
  </macro>
  <macro name="date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="access">
    <choose>
      <if variable="DOI"/>
      <else>
        <choose>
          <if variable="URL">
            <group delimiter=" " prefix=" ">
              <group>
                <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
              </group>
              <group prefix="[" suffix="]" delimiter=" ">
                <date variable="accessed">
                  <date-part name="day" prefix="&lt;day&gt;" suffix="&lt;/day&gt;"/>
                  <date-part name="month" prefix="&lt;month&gt; " suffix=" &lt;/month&gt;" form="short"/>
                  <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
                </date>
              </group>
            </group>
          </if>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if variable="DOI">
        <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </if>
      <else>
        <group delimiter=" ">
          <choose>
            <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
              <choose>
                <if variable="URL">
                  <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                </if>
                <else>
                  <text variable="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
                </else>
              </choose>
            </if>
            <else>
              <text variable="title" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
            </else>
          </choose>
          <choose>
            <if variable="URL">
              <text term="online" prefix="[" suffix="]" text-case="capitalize-first"/>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix=".&lt;/edition&gt;" prefix="&lt;edition&gt;"/>
      </else>
    </choose>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" collapse="year">
    <sort>
      <key macro="date"/>
      <key macro="author-bib"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter="; ">
      <group delimiter=" ">
        <text macro="author-cit"/>
        <text macro="date"/>
      </group>
    </layout>
  </citation>
  <bibliography>
    <sort>
      <key macro="author-bib"/>
      <key macro="date"/>
    </sort>
    <layout suffix=".">
      <text macro="author-bib"/>
      <text macro="title" suffix=". "/>
      <choose>
        <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
          <group delimiter=" " suffix=".">
            <text macro="edition"/>
            <text macro="publisher"/>
          </group>
        </if>
        <else-if type="chapter paper-conference" match="any">
          <group delimiter=". ">
            <group>
              <text term="in" suffix=": " text-case="capitalize-first"/>
              <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
              <text macro="editor" prefix=", "/>
            </group>
            <text variable="event" prefix="&lt;event&gt;" suffix="&lt;/event&gt;"/>
            <text macro="publisher"/>
          </group>
          <text variable="page" prefix="&lt;page&gt;, p. " suffix=".&lt;/page&gt;"/>
        </else-if>
        <else-if type="article-journal">
          <choose>
            <if variable="issue volume" match="any">
              <text variable="container-title" suffix=" &lt;/container-title&gt;" form="short" font-style="italic" strip-periods="true" prefix="&lt;container-title&gt;"/>
              <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
              <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
              <text macro="date" prefix=", " suffix="."/>
            </if>
            <else>
              <choose>
                <if variable="DOI">
                  <text variable="container-title" suffix=" &lt;/container-title&gt;" form="short" font-style="italic" prefix="&lt;container-title&gt;"/>
                  <group prefix="(" suffix=").">
                    <date variable="issued">
                      <date-part name="month" prefix="&lt;month&gt; " suffix=" &lt;/month&gt;"/>
                      <date-part name="day" suffix=", &lt;/day&gt;" prefix="&lt;day&gt;"/>
                      <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
                    </date>
                  </group>
                  <text variable="DOI" prefix="&lt;DOI&gt; doi: " suffix="&lt;/DOI&gt;"/>
                </if>
                <else>
                  <text variable="container-title" suffix=". &lt;/container-title&gt;" form="short" font-style="italic" prefix="&lt;container-title&gt;"/>
                </else>
              </choose>
            </else>
          </choose>
        </else-if>
        <else>
          <text variable="container-title" suffix=" &lt;/container-title&gt;" form="short" font-style="italic" prefix="&lt;container-title&gt;"/>
          <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
          <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          <text variable="page" prefix="&lt;page&gt;: " suffix=", &lt;/page&gt;"/>
          <text macro="date" suffix="."/>
        </else>
      </choose>
      <text macro="access"/>
    </layout>
  </bibliography>
</style>