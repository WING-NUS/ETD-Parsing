<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" demote-non-dropping-particle="sort-only" version="1.0" class="in-text" default-locale="en-US">
  <info>
    <title>Optics Express</title>
    <id>http://www.zotero.org/styles/optics-express</id>
    <link href="http://www.zotero.org/styles/optics-express" rel="self"/>
    <link href="http://www.zotero.org/styles/the-optical-society" rel="template"/>
    <link href="https://www.osapublishing.org/submit/style/oestyleguide.cfm" rel="documentation"/>
    <author>
      <name>Lawrence Yip</name>
      <email>lyip4@uwo.ca</email>
    </author>
    <category citation-format="numeric"/>
    <category field="physics"/>
    <eissn>1094-4087</eissn>
    <summary>Common style use by OSA Express publications.</summary>
    <updated>2018-01-31T22:34:37+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name and="text" delimiter=", " initialize-with=". "/>
      <label prefix=", " form="short"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;">
      <name and="text" delimiter=", " initialize-with=". "/>
      <label prefix=", " form="short"/>
    </names>
  </macro>
  <macro name="year-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="day-date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="month" form="long" suffix=" &lt;/month&gt;" prefix="&lt;month&gt;"/>
          <date-part name="day" suffix=", &lt;/day&gt;" prefix="&lt;day&gt;"/>
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text form="short" term="no date"/>
      </else>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", " prefix="(" suffix=")">
      <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      <text macro="year-date"/>
    </group>
  </macro>
  <macro name="volume">
    <text variable="volume" font-weight="bold" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
    <number prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;" variable="issue"/>
  </macro>
  <macro name="edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" " prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;">
          <number variable="edition" form="ordinal"/>
          <text form="short" term="edition"/>
        </group>
      </if>
      <else>
        <text variable="edition" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="series">
    <group delimiter=" ">
      <text variable="collection-title" text-case="title" prefix="&lt;collection-title&gt;" suffix="&lt;/collection-title&gt;"/>
      <text variable="collection-number" prefix="&lt;collection-number&gt;No. " suffix="&lt;/collection-number&gt;"/>
    </group>
  </macro>
  <macro name="personal-communication">
    <group prefix="personal communication (" suffix=")">
      <text macro="day-date"/>
    </group>
  </macro>
  <macro name="patent-authority-number">
    <choose>
      <if variable="authority">
        <group>
          <text variable="authority" suffix=" patent &lt;/authority&gt;" prefix="&lt;authority&gt;"/>
          <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
        </group>
      </if>
      <else>
        <group>
          <text variable="number" prefix="&lt;number&gt;U.S. patent " suffix="&lt;/number&gt;"/>
        </group>
      </else>
    </choose>
  </macro>
  <citation collapse="citation-number">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout delimiter="," prefix="[" suffix="]">
      <text variable="citation-number" prefix="&lt;citation-number&gt;" suffix="&lt;/citation-number&gt;"/>
    </layout>
  </citation>
  <bibliography second-field-align="flush" entry-spacing="0">
    <layout suffix=".">
      <text variable="citation-number" suffix=". &lt;/citation-number&gt;" prefix="&lt;citation-number&gt;"/>
      <text macro="author" suffix=", "/>
      <choose>
        <if match="any" type="bill book graphic legal_case legislation motion_picture report song">
          <group delimiter=" ">
            <group delimiter=", ">
              <text variable="title" text-case="title" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
              <text macro="edition"/>
              <text macro="series"/>
            </group>
            <group delimiter=", ">
              <text macro="publisher"/>
              <text macro="volume"/>
              <group delimiter=" ">
                <label variable="page" form="short"/>
                <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
              </group>
            </group>
          </group>
        </if>
        <else-if type="paper-conference">
          <group delimiter=" ">
            <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot; &lt;/title&gt;"/>
            <text term="in"/>
            <group delimiter=", ">
              <text variable="container-title" form="long" text-case="title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
              <text macro="editor"/>
              <text macro="series"/>
            </group>
            <group delimiter=", ">
              <text macro="publisher"/>
              <text macro="volume"/>
              <group delimiter=" ">
                <label variable="page" form="short"/>
                <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
              </group>
            </group>
          </group>
        </else-if>
        <else-if type="speech">
          <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot;&lt;/title&gt;"/>
          <group delimiter=", ">
            <text variable="event" prefix="&lt;event&gt; presented at " form="long" text-case="title" suffix="&lt;/event&gt;"/>
            <text variable="event-place" text-case="title" prefix="&lt;event-place&gt;" suffix="&lt;/event-place&gt;"/>
          </group>
          <text prefix=" (" suffix=")" macro="day-date"/>
        </else-if>
        <else-if match="any" type="chapter paper-conference">
          <group delimiter=" ">
            <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot;&lt;/title&gt;"/>
            <text term="in"/>
            <group delimiter=", ">
              <text variable="container-title" form="long" text-case="title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
              <text macro="editor"/>
              <text macro="edition"/>
              <text macro="series"/>
            </group>
            <group delimiter=", ">
              <text macro="publisher"/>
              <text macro="volume"/>
              <group delimiter=" ">
                <label variable="page" form="short"/>
                <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
              </group>
            </group>
          </group>
        </else-if>
        <else-if type="patent">
          <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot; &lt;/title&gt;"/>
          <group delimiter=" ">
            <text macro="patent-authority-number"/>
            <text prefix=" (" suffix=")" macro="day-date"/>
          </group>
        </else-if>
        <else-if type="thesis">
          <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot; &lt;/title&gt;"/>
          <group delimiter=", ">
            <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
            <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
          </group>
          <text prefix=" (" suffix=")" macro="year-date"/>
        </else-if>
        <else-if type="webpage">
          <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot; &lt;/title&gt;"/>
          <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
        </else-if>
        <else-if type="personal_communication">
          <text macro="personal-communication"/>
        </else-if>
        <else-if type="article-newspaper">
          <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot; &lt;/title&gt;"/>
          <text variable="container-title" form="long" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          <text prefix=" (" suffix=")" macro="day-date"/>
        </else-if>
        <else-if type="report">
          <group delimiter=" ">
            <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot;&lt;/title&gt;"/>
            <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
            <text variable="number" prefix="&lt;number&gt;" suffix="&lt;/number&gt;"/>
            <text variable="publisher" prefix="&lt;publisher&gt;(" suffix="&lt;/publisher&gt;"/>
            <text prefix=", " suffix=")," macro="day-date"/>
            <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
          </group>
        </else-if>
        <else>
          <group delimiter=" ">
            <text variable="title" prefix="&lt;title&gt;&quot;" suffix=",&quot;&lt;/title&gt;"/>
            <text variable="container-title" form="short" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <group delimiter=", ">
              <text macro="volume"/>
              <group delimiter=" ">
                <text variable="page" form="short" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
                <text prefix="(" suffix=")" macro="year-date"/>
              </group>
            </group>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
</style>