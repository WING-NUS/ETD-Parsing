  <locale xml:lang="de">
    <terms>
      <term name="et-al">u.a.</term>
      <term name="and">und</term>
      <term name="retrieved">zugegriffen am</term>
      <term name="accessed">abgerufen am</term>
      <term name="ibid">ebenda</term>
      <term name="page" form="short">S.</term>
      <term name="section" form="short">Abs.</term>
      <term name="editor" form="verb-short">hrsg. von</term>
      <term name="editor" form="short">(Hrsg.)</term>
    </terms>
  </locale>
  
  <macro name="author">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name and="text" name-as-sort-order="all" sort-separator=", " delimiter=", " delimiter-precedes-last="never"/>
      <label form="short" text-case="lowercase" strip-periods="true"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="editor">
    <names variable="editor translator" prefix="&lt;editor translator&gt;" suffix="&lt;/editor translator&gt;">
      <name and="text" name-as-sort-order="all" sort-separator=", " delimiter=", " delimiter-precedes-last="never"/>
      <label form="short" prefix=" " strip-periods="true"/>
    </names>
  </macro>
  <macro name="accessed">
    <text term="accessed"/>
    <date variable="accessed" prefix=" ">
      <date-part form="numeric-leading-zeros" name="day" suffix=".&lt;/day&gt;" prefix="&lt;day&gt;"/>
      <date-part form="numeric-leading-zeros" name="month" suffix=".&lt;/month&gt;" prefix="&lt;month&gt;"/>
      <date-part form="long" name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <macro name="date">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part form="numeric-leading-zeros" name="day" suffix=".&lt;/day&gt;" prefix="&lt;day&gt;"/>
          <date-part form="numeric-leading-zeros" name="month" suffix=".&lt;/month&gt;" prefix="&lt;month&gt;"/>
          <date-part form="long" name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <date variable="accessed">
          <date-part form="numeric-leading-zeros" name="day" suffix=".&lt;/day&gt;" prefix="&lt;day&gt;"/>
          <date-part form="numeric-leading-zeros" name="month" suffix=".&lt;/month&gt;" prefix="&lt;month&gt;"/>
          <date-part form="long" name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="year-date">
    <choose>
      <if type="webpage">
        <choose>
          <if variable="issued">
            <date variable="issued">
              <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
            </date>
          </if>
          <else>
            <date variable="accessed">
              <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
            </date>
          </else>
        </choose>
      </if>
      <else>
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="pages">
    <label variable="locator" form="short" prefix=", " suffix=" "/>
    <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
  </macro>
  <macro name="point-locators">
    <label variable="locator" form="short" prefix=", " suffix=" "/>
    <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
  </macro>
  <macro name="point-locators-subsequent">
    <group>
      <choose>
        <if locator="page" match="none">
          <choose>
            <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
              <choose>
                <if variable="volume">
                  <group prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;">
                    <text term="volume" form="short" text-case="lowercase" suffix=" "/>
                    <number variable="volume" form="numeric"/>
                    <label variable="locator" form="short" prefix=", "/>
                  </group>
                </if>
                <else>
                  <label variable="locator" form="short" suffix=" "/>
                </else>
              </choose>
            </if>
          </choose>
        </if>
        <else-if type="bill book graphic legal_case legislation motion_picture report song" match="any">
          <number variable="volume" form="numeric" suffix=":&lt;/volume&gt;" prefix="&lt;volume&gt;"/>
        </else-if>
      </choose>
      <label variable="locator" form="short" suffix=" "/>
      <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
    </group>
  </macro>
  <macro name="hasUrl">
    <choose>
      <if variable="URL" match="any">
        <text value="yes"/>
      </if>
      <else>
        <text value="no"/>
      </else>
    </choose>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true">
    <layout delimiter="; " suffix=".">
      <choose>
        <if position="ibid">
          <text term="ibid"/>
          <text macro="point-locators-subsequent" prefix=", "/>
        </if>
        <else>
          <text macro="author"/>
          <text macro="year-date" prefix=" (" suffix=")"/>
          <text macro="point-locators"/>
          <choose>
            <if variable="URL" match="any">
              <text value="(siehe Internet-/Intranetverzeichnis)" prefix=" "/>
            </if>
          </choose>
        </else>
      </choose>
    </layout>
  </citation>
  <bibliography hanging-indent="true">
    <sort>
      <key macro="hasUrl"/>
      <key macro="author"/>
      <key macro="date"/>
    </sort>
    <layout suffix=".">
      <group font-weight="bold">
        <text macro="author"/>
        <text macro="year-date" prefix=" (" suffix="):" font-weight="bold"/>
      </group>
      <text variable="title" prefix="&lt;title&gt; " suffix="&lt;/title&gt;"/>
      <choose>
        <if type="webpage">
          <text macro="date" prefix=", "/>
        </if>
        <else-if type="speech" match="any">
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;, " suffix="&lt;/publisher-place&gt;"/>
          <text macro="date" prefix=", "/>
          <text macro="accessed" prefix=", "/>
        </else-if>
        <else-if type="article-newspaper article-magazine article-journal" match="any">
          <text value=" In: " font-style="italic"/>
          <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          <text variable="collection-title" prefix="&lt;collection-title&gt;, Reihe " suffix="&lt;/collection-title&gt;"/>
          <text variable="volume" prefix="&lt;volume&gt;, Band " suffix="&lt;/volume&gt;"/>
          <text variable="issue" prefix="&lt;issue&gt;, Ausgabe " suffix="&lt;/issue&gt;"/>
          <text macro="date" prefix=", "/>
          <text macro="pages"/>
        </else-if>
        <else-if type="chapter paper-conference" match="any">
          <text macro="editor" prefix=", in: " suffix=": "/>
          <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;, " suffix="&lt;/publisher-place&gt;"/>
          <text variable="edition" prefix="&lt;edition&gt;, " suffix="&lt;/edition&gt;"/>
          <text macro="year-date" prefix=", "/>
          <text macro="pages"/>
        </else-if>
        <else>
          <text variable="edition" prefix="&lt;edition&gt;, " suffix="&lt;/edition&gt;"/>
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;, " suffix="&lt;/publisher-place&gt;"/>
          <text variable="publisher" prefix="&lt;publisher&gt;, " suffix="&lt;/publisher&gt;"/>
          <text macro="year-date" prefix=", "/>
        </else>
      </choose>
      <choose>
        <if variable="URL">
          <text macro="accessed" prefix=", "/>
          <text variable="URL" prefix="&lt;URL&gt;, " suffix="&lt;/URL&gt;"/>
        </if>
      </choose>
      <text variable="ISBN" prefix="&lt;ISBN&gt;, ISBN: " suffix="&lt;/ISBN&gt;"/>
      <text variable="DOI" prefix="&lt;DOI&gt;, DOI: " suffix="&lt;/DOI&gt;"/>
      <text variable="note" prefix="&lt;note&gt;, " suffix="&lt;/note&gt;"/>
    </layout>
  </bibliography>
</style>