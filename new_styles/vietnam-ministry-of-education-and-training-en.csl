<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" et-al-min="4" et-al-use-first="3" demote-non-dropping-particle="sort-only" default-locale="en-US">
  <info>
    <title>Vietnam Ministry of Education and Training (English)</title>
    <title-short>BGDDT_TA</title-short>
    <id>http://www.zotero.org/styles/vietnam-ministry-of-education-and-training-en</id>
    <link href="http://www.zotero.org/styles/vietnam-ministry-of-education-and-training-en" rel="self"/>
    <link href="http://www.zotero.org/styles/acm-sigchi-proceedings" rel="template"/>
    <link href="http://www.vdic.org.vn/en/library/guides.html" rel="documentation"/>
    <author>
      <name>Pham Thanh Tung</name>
      <email>tung.pham@fsh.org.vn</email>
      <uri>http://fsh.org.vn</uri>
    </author>
    <contributor>
      <name>Bùi Phương Linh</name>
      <email>linh.bui@fsh.org.vn</email>
      <uri>http://fsh.org.vn</uri>
    </contributor>
    <category citation-format="numeric"/>
    <summary>Style Bộ giáo dục đào tạo Việt Nam (Tiếng Anh)</summary>
    <updated>2015-05-09T09:52:43+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="author">
    <group suffix=" ">
      <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
        <name and="text" delimiter-precedes-et-al="always" initialize-with="." name-as-sort-order="all" sort-separator=" "/>
        <label form="short" prefix=", " suffix="." strip-periods="true"/>
        <substitute>
          <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        </substitute>
      </names>
    </group>
    <choose>
      <if type="book thesis legislation report legal_case" match="any">
        <text macro="year" prefix="(" suffix="), "/>
      </if>
      <else>
        <text macro="year" prefix="(" suffix="). "/>
      </else>
    </choose>
  </macro>
  <macro name="title">
    <choose>
      <if type="book thesis legislation report" match="any">
        <text variable="title" quotes="false" font-style="italic" suffix=", &lt;/title&gt;" prefix="&lt;title&gt;"/>
      </if>
      <else>
        <text variable="title" quotes="false" suffix=". &lt;/title&gt;" prefix="&lt;title&gt;"/>
      </else>
    </choose>
  </macro>
  <macro name="year">
    <date date-parts="year" form="numeric" variable="issued">
      <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
    </date>
  </macro>
  <macro name="conference">
    <group delimiter=", ">
      <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
      <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
      <date form="numeric" variable="event-date" prefix="&lt;event-date&gt;" suffix="&lt;/event-date&gt;"/>
      <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
    </group>
  </macro>
  <macro name="book-publisher">
    <group delimiter=", ">
      <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
      <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
      <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
    </group>
  </macro>
  <citation near-note-distance="2">
    <sort>
      <key variable="citation-number"/>
    </sort>
    <layout font-variant="normal" delimiter="], [" prefix=" [" suffix="]">
      <group delimiter=", ">
        <text variable="citation-number" form="short" strip-periods="false" quotes="false" font-variant="normal" prefix="&lt;citation-number&gt;" suffix="&lt;/citation-number&gt;"/>
        <text variable="locator" prefix="&lt;locator&gt;" suffix="&lt;/locator&gt;"/>
      </group>
    </layout>
  </citation>
  <bibliography and="text" delimiter-precedes-et-al="always" initialize-with="." sort-separator="." second-field-align="flush" entry-spacing="0">
    <layout suffix=".">
      <text variable="citation-number" suffix=".&lt;/citation-number&gt;" prefix="&lt;citation-number&gt;"/>
      <text macro="author" strip-periods="false" suffix=" "/>
      <text macro="title"/>
      <choose>
        <if type="bill book graphic legal_case legislation motion_picture report song thesis" match="any">
          <text macro="book-publisher" suffix="."/>
        </if>
        <else-if type="paper-conference">
          <group suffix="." delimiter=", ">
            <text macro="conference"/>
            <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
          </group>
        </else-if>
        <else-if type="chapter" match="any">
          <text variable="container-title" font-style="italic" suffix=". &lt;/container-title&gt;" prefix="&lt;container-title&gt;"/>
          <group suffix="." delimiter=", ">
            <text variable="edition" prefix="&lt;edition&gt;" suffix="&lt;/edition&gt;"/>
            <text macro="book-publisher"/>
            <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
          </group>
        </else-if>
        <else-if type="article-journal">
          <group delimiter=", " suffix=".">
            <text variable="container-title" form="short" strip-periods="true" font-style="italic" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <group>
              <text variable="volume" font-weight="bold" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
              <text variable="issue" font-weight="bold" prefix="&lt;issue&gt;(" suffix=")&lt;/issue&gt;"/>
            </group>
            <text variable="page" form="short" strip-periods="false" font-weight="normal" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
          </group>
        </else-if>
        <else>
          <group delimiter=", ">
            <text variable="container-title" prefix="&lt;container-title&gt;" suffix="&lt;/container-title&gt;"/>
            <text variable="volume" prefix="&lt;volume&gt;" suffix="&lt;/volume&gt;"/>
            <text variable="page" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
            <text variable="URL" prefix="&lt;URL&gt;&lt;" suffix="&gt;&lt;/URL&gt;"/>
            <date form="numeric" variable="accessed" prefix="&lt;accessed&gt;accessed: " suffix="&lt;/accessed&gt;"/>
          </group>
        </else>
      </choose>
    </layout>
  </bibliography>
  <locale xml:lang="en-US"/>
</style>