<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" class="in-text" version="1.0" demote-non-dropping-particle="sort-only" default-locale="en-GB">
  
  <info>
    <title>World's Poultry Science Journal</title>
    <title-short>WPSJ</title-short>
    <id>http://www.zotero.org/styles/worlds-poultry-science-journal</id>
    <link href="http://www.zotero.org/styles/worlds-poultry-science-journal" rel="self"/>
    <link href="http://www.zotero.org/styles/ecology" rel="template"/>
    <link href="http://assets.cambridge.org/WPS/WPS_ifc.pdf" rel="documentation"/>
    <author>
      <name>Andy Wales</name>
    </author>
    <category citation-format="author-date"/>
    <category field="biology"/>
    <issn>0043-9339</issn>
    <eissn>1743-4777</eissn>
    <updated>2016-06-20T11:03:25+00:00</updated>
    <rights license="http://creativecommons.org/licenses/by-sa/3.0/">This work is licensed under a Creative Commons Attribution-ShareAlike 3.0 License</rights>
  </info>
  <macro name="container-contributors">
    <choose>
      <if type="chapter paper-conference" match="any">
        <text term="in" font-style="normal" suffix=" "/>
        <names variable="editor translator" delimiter=", " suffix="  &lt;/editor translator&gt;" prefix="&lt;editor translator&gt;">
          <name and="text" initialize-with=". ">
            <name-part name="family" text-case="uppercase"/>
          </name>
          <label form="short" text-case="capitalize-first" strip-periods="true" prefix=" (" suffix=") "/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="secondary-contributors">
    <choose>
      <if type="chapter paper-conference" match="none">
        <names variable="editor translator" delimiter=", " prefix="&lt;editor translator&gt; (" suffix=")&lt;/editor translator&gt;">
          <name and="text" initialize-with=". " delimiter=", "/>
          <label form="short" prefix=", " text-case="capitalize-first"/>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="author">
    <names variable="author" font-weight="bold" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name font-weight="bold" and="text" delimiter-precedes-last="never" initialize-with=". " name-as-sort-order="all">
        <name-part name="family" text-case="uppercase" font-weight="bold"/>
      </name>
      <label prefix=", "/>
      <substitute>
        <names variable="editor" font-weight="normal" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" font-weight="normal" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <text variable="title" text-case="uppercase" font-weight="bold" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      </substitute>
    </names>
  </macro>
  <macro name="author-short">
    <names variable="author" prefix="&lt;author&gt;" suffix="&lt;/author&gt;">
      <name form="short" and="text" delimiter=", " initialize-with=". "/>
      <et-al font-style="italic"/>
      <substitute>
        <names variable="editor" prefix="&lt;editor&gt;" suffix="&lt;/editor&gt;"/>
        <names variable="translator" prefix="&lt;translator&gt;" suffix="&lt;/translator&gt;"/>
        <choose>
          <if type="bill book graphic legal_case legislation motion_picture report song" match="any">
            <text variable="title" form="short" font-style="italic" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </if>
          <else>
            <text variable="title" form="short" quotes="true" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="access">
    <choose>
      <if type="webpage report" match="any">
        <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
        <date form="text" variable="accessed" prefix="&lt;accessed&gt; (accessed " suffix=")&lt;/accessed&gt;"/>
      </if>
    </choose>
  </macro>
  <macro name="publisher">
    <group delimiter=", " prefix=" (" suffix=")">
      <choose>
        <if type="article-journal article-magazine paper-conference" match="none">
          <text variable="genre" prefix="&lt;genre&gt;" suffix="&lt;/genre&gt;"/>
          <text variable="publisher-place" prefix="&lt;publisher-place&gt;" suffix="&lt;/publisher-place&gt;"/>
          <text variable="publisher" prefix="&lt;publisher&gt;" suffix="&lt;/publisher&gt;"/>
        </if>
      </choose>
    </group>
  </macro>
  <macro name="issued">
    <choose>
      <if variable="issued">
        <group prefix=" " suffix=".">
          <date variable="issued">
            <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
          </date>
          <choose>
            <if type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song" match="none">
              <date variable="issued">
                <date-part prefix="&lt;month&gt;, " name="month" suffix="&lt;/month&gt;"/>
                <date-part prefix="&lt;day&gt; " name="day" suffix="&lt;/day&gt;"/>
              </date>
            </if>
          </choose>
        </group>
      </if>
      <else>
        <text prefix=" (" term="no date" suffix=")." form="short"/>
      </else>
    </choose>
  </macro>
  <macro name="issued-year">
    <choose>
      <if variable="issued">
        <date variable="issued">
          <date-part name="year" prefix="&lt;year&gt;" suffix="&lt;/year&gt;"/>
        </date>
      </if>
      <else>
        <text term="no date" form="short"/>
      </else>
    </choose>
  </macro>
  <macro name="edition">
    <choose>
      <if type="bill book chapter graphic legal_case legislation motion_picture paper-conference report song" match="any">
        <choose>
          <if is-numeric="edition">
            <number text-case="capitalize-first" prefix="&lt;edition&gt;. " variable="edition" form="long-ordinal" suffix="&lt;/edition&gt;"/>
            <text term="edition" form="long" prefix=" " suffix="."/>
          </if>
          <else>
            <text variable="edition" suffix=".&lt;/edition&gt;" prefix="&lt;edition&gt;"/>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="locators">
    <choose>
      <if type="article-journal article-magazine article-newspaper" match="any">
        <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;. " suffix="&lt;/container-title&gt;"/>
        <text variable="volume" font-weight="bold" prefix="&lt;volume&gt; " suffix="&lt;/volume&gt;"/>
        <text variable="page" prefix="&lt;page&gt;: " suffix="&lt;/page&gt;"/>
      </if>
      <else-if type="bill chapter graphic legal_case legislation motion_picture song" match="any">
        <group delimiter=" " prefix=", ">
          <text macro="container-contributors"/>
          <text macro="secondary-contributors"/>
          <text variable="container-title" strip-periods="true" font-style="italic" suffix=",&lt;/container-title&gt;" prefix="&lt;container-title&gt;"/>
          <choose>
            <if match="all" variable="volume">
              <label plural="never" text-case="capitalize-first" variable="volume" form="short"/>
              <text variable="volume" form="short" suffix=", &lt;/volume&gt;" prefix="&lt;volume&gt;"/>
            </if>
          </choose>
          <label variable="page" form="short"/>
          <text variable="page" strip-periods="false" prefix="&lt;page&gt;" suffix="&lt;/page&gt;"/>
        </group>
      </else-if>
      <else-if type="paper-conference" match="any">
        <group>
          <text variable="container-title" font-style="italic" prefix="&lt;container-title&gt;. In: " suffix=". &lt;/container-title&gt;"/>
          <date form="text" variable="issued" suffix=", &lt;/issued&gt;" prefix="&lt;issued&gt;"/>
          <text variable="event-place" suffix=", &lt;/event-place&gt;" prefix="&lt;event-place&gt;"/>
          <label suffix=" " variable="page" form="short"/>
          <text variable="page" suffix=". &lt;/page&gt;" prefix="&lt;page&gt;"/>
          <text variable="URL" prefix="&lt;URL&gt;" suffix="&lt;/URL&gt;"/>
          <date form="text" variable="accessed" prefix="&lt;accessed&gt; (accessed " suffix=")&lt;/accessed&gt;"/>
        </group>
      </else-if>
    </choose>
  </macro>
  <macro name="citation-locator">
    <group>
      <label variable="locator" form="short"/>
      <text variable="locator" prefix="&lt;locator&gt; " suffix="&lt;/locator&gt;"/>
    </group>
  </macro>
  <citation et-al-min="3" et-al-use-first="1" disambiguate-add-year-suffix="true" collapse="year">
    <sort>
      <key variable="issued"/>
      <key macro="author"/>
    </sort>
    <layout prefix="(" suffix=")" delimiter=", ">
      <group delimiter=" ">
        <text macro="author-short"/>
        <text macro="issued-year"/>
        <text macro="citation-locator"/>
      </group>
    </layout>
  </citation>
  <bibliography entry-spacing="0" line-spacing="2">
    <sort>
      <key macro="author"/>
      <key macro="issued-year"/>
    </sort>
    <layout suffix=".">
      <text macro="author" font-weight="bold" suffix="."/>
      <text macro="issued" suffix=" "/>
      <text variable="title" strip-periods="false" prefix="&lt;title&gt;" suffix="&lt;/title&gt;"/>
      <text macro="locators"/>
      <group delimiter=". ">
        <text macro="edition"/>
        <text macro="publisher"/>
        <choose>
          <if type="book" match="any">
            <text variable="number-of-pages" suffix=" pp&lt;/number-of-pages&gt;" prefix="&lt;number-of-pages&gt;"/>
          </if>
          <else-if type="report" match="any">
            <text variable="page" suffix=" pp&lt;/page&gt;" prefix="&lt;page&gt;"/>
          </else-if>
        </choose>
        <text macro="access"/>
      </group>
    </layout>
  </bibliography>
</style>